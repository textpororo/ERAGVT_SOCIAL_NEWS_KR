@list()
	진지함_リスト数=0
	진지함_リスト:0=

@li(st)
#DIMS st
	진지함_リスト:(진지함_リスト数)=%st%
	진지함_リスト数+=1

@end()
	SIF 진지함_リスト数==0
		RETURN
	LOCALS=%진지함_リスト:(RAND:(진지함_リスト数))%
	CALL ss(LOCALS)
	CALL list()

@ss(st)
#DIMS st

#DIM startp
#DIM endp

;?r(あいう/えおか/きくけ)
WHILE 1
	STRFIND st,"\?r\("
	startp=RESULT
	IF startp>=0
		STRFIND st,"\)",startp+1
		endp=RESULT
		IF endp>=0
			SUBSTRING st,startp+3,(endp-startp-3)
			LOCALS=%RESULTS%
			SPLIT LOCALS,"/",LOCALS
			LOCALS=%LOCALS:(RAND:(RESULT))%
	
			SUBSTRING st,0,startp
			LOCALS:1=%RESULTS%
			SUBSTRING st,endp+1,-1
			LOCALS:2=%RESULTS%

			st=%LOCALS:1+LOCALS+LOCALS:2%
		ELSE
			BREAK
		ENDIF
	ELSE
		BREAK
	ENDIF
WEND



