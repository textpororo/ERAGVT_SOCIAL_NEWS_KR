;==================================================
;【전용 루틴（성행위）】
;--------------------------------------------------
@TENTACLE_CITIZEN_1150_SEX_ROUTINE
LOCAL = RAND:100
;모유 흡입
IF LOCAL < 15 && TALENT:모유체질
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		IF ISHOLE() == 0
			RETURN 1
		ELSE
			RETURN 7
		ENDIF
	ENDIF
;전신 애무
ELSEIF LOCAL < 20
	RETURN 0
;Ｃ공격（강）
ELSEIF LOCAL < 40
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		RETURN 1
	ENDIF
;펠라
ELSEIF LOCAL < 60
	IF ISHOLE() == 0
		RETURN 1
	ELSE
		RETURN 11
	ENDIF
;Ｖ공격（약）
ELSEIF LOCAL < 80
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		IF ISMALE()
			RETURN 1
		ELSE
			RETURN 2
		ENDIF
	ENDIF
;씨받이 교배
ELSEIF (LOCAL < 95 || (TFLAG:20 == 3 && LOCAL < 90) || (TCVARn:12 & 배란) > 0) && CHECK_HOLYVIRGIN_F()==0
	IF CLOTH_OUTER_PER > 90 || CLOTH_INNER_PER >= CLOTH_INNER_DEF
		RETURN 14
	ELSE
		IF ISMALE()
			RETURN 1
		ELSE
			RETURN 3
		ENDIF
	ENDIF
;약물 주사
ELSE
	RETURN 9
ENDIF
