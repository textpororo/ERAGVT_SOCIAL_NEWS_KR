;교배 피스톤（Ｖ계통으로부터의 파생 한정 행동）

;성 커맨드 분류
@SEX_TYPE_COM15
RETURN 삽입

@SEX_COM15,ARG = 0,ARG:1 = 0
;EX_COM	추가 고문 실행 부위（비트）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;추가 고문
CALL SEX_COMEX_RANDOM, Ｖ, ARG:1
EX_COM = RESULT:0
SH_COM = RESULT:1

INSERT |= 질
;상태 이상：강구속
SIF (TCVARn:12 & 강구속) == 0
	CALL STATE_CHANGE_PKOUSOKU, 100

;흑화 캐릭터의 페니스 위치를 세트
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && (ISPENIS(FLAG:111))
	TFLAG:18 = 질

;취득 파라미터 계산
;BASE값의 명시적 초기화
REPEAT 12
	LOCAL:COUNT = 0
REND

;방심도 상승
TFLAG:3 += 5

;사정치 450
CALL TENTACLE_SYASEI_UP, 450

;촉수 크기 설정
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 15

;쾌Ｖ
IF ABL:Ｖ감각 == 0
	LOCAL:1 = 40
ELSEIF ABL:Ｖ감각 == 1
	LOCAL:1 = 100
ELSEIF ABL:Ｖ감각 == 2
	LOCAL:1 = 200
ELSEIF ABL:Ｖ감각 == 3
	LOCAL:1 = 400
ELSEIF ABL:Ｖ감각 == 4
	LOCAL:1 = 1000
ELSEIF ABL:Ｖ감각 >= 5
	LOCAL:1 = 2000
ENDIF
;메인 부위 절정 예정의 예측
CALL PALAM_VABCestimate(LOCAL,1,-1)


IF ENEMY_TYPE_CHECK_F("MOB") == 1 || ENEMY_TYPE_CHECK_F("CITIZEN") == 1
	TRYCALLFORM MESSAGE_MOB_{ARG}_COM15, EX_COM, SH_COM
ELSE
	;지문: 세뇌/흑화 캐릭터가 조작 캐릭터에게 교배 피스톤을 했다
	SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
		CALL MESSAGE_OTHER_SEX_COM15
	;지문: COM2
	CALL MESSAGE_SEX_COM15, EX_COM, SH_COM
ENDIF

;결계에 따른 촉수 개수의 변동
CALL SET_TENTACLE_POOL
;의상에의 피해
CALL CLOTH_BATTLE_DAMAGE, 2

;BASE
EXP:Ｖ경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 0 && INCEST_F(TARGET, FLAG:10 * 100 + FLAG:11, 1) > 0
	EXP:근친교배경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && INCEST_F(TARGET, FLAG:111) > 0
	EXP:근친교배경험 += 1



;공순
IF ABL:순종 <= 2
	LOCAL:5 = 20
ELSEIF ABL:순종 == 3
	LOCAL:5 = 50
ELSEIF ABL:순종 == 4
	LOCAL:5 = 100
ELSEIF ABL:순종 >= 5
	LOCAL:5 = 200
ENDIF

;습득
IF ABL:봉사정신 == 0
	LOCAL:6 = 10
ELSEIF ABL:봉사정신 == 1
	LOCAL:6 = 25
ELSEIF ABL:봉사정신 == 2
	LOCAL:6 = 50
ELSEIF ABL:봉사정신 == 3
	LOCAL:6 = 100
ELSEIF ABL:봉사정신 == 4
	LOCAL:6 = 250
ELSEIF ABL:봉사정신 >= 5
	LOCAL:6 = 500
ENDIF

;윤활이 낮으면 고통이 들어간다
IF PALAM:윤활 < 1000
	LOCAL:10 += 1000
ELSEIF PALAM:윤활 < 2000
	LOCAL:10 += 200
ENDIF

;Ｖ감각이 낮으면 공포가 들어간다
IF ABL:Ｖ감각 == 0
	LOCAL:11 = 400
ELSEIF ABL:Ｖ감각 == 1
	LOCAL:11 = 100
ELSEIF ABL:Ｖ감각 == 2
	LOCAL:11 = 40
ELSEIF ABL:Ｖ감각 == 3
	LOCAL:11 = 20
ELSE
ENDIF

;추가 고문 실행
IF EX_COM
	CALL SEX_COMEX, Ｖ, 1, EX_COM | SH_COM
	REPEAT 12
		LOCAL:COUNT += RESULT:COUNT
	REND
ENDIF

;대량 사정 가능하면 피니쉬로 이행
IF FLAG:15 >= FLAG:14 * 2
	TFLAG:17 = 16
;Ａ추가 고문이 서 있으면 양 구멍 피스톤으로 이행
ELSEIF (EX_COM & Ａ) && FLAG:15 >= FLAG:14 && BASE:Ａ결계내구력 == 0 && (ENEMY_TYPE_CHECK_F("MOB") == 0 && ENEMY_TYPE_CHECK_F("CITIZEN") == 0) && ISHOLE() == 1
	TFLAG:17 = 19
;그 이외라면 교배 이행
ELSE
	TFLAG:17 = 15
ENDIF
;촉수의 행동을 기록
TFLAG:20 = 15

;습득 기본값
LOCAL:6 += 150

;욕정 기본값
LOCAL:7 += 100

;굴복 기본값
LOCAL:8 += 100

;치정 기본값
LOCAL:9 += 100

;고통 기본값
LOCAL:10 += 50

;공포 기본값
LOCAL:11 += 150

;체력 소비 기본값
LOCAL:12 += 1

;계산으로
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
