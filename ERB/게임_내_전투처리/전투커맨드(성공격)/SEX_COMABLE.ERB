;**********************************************************
;성 커맨드의 COMABLE
;캐릭터의 신체적 특징에 따라 실행 불가능한 커맨드를 다른 커맨드로 배정하다
;ARG=커맨드 번호
@SEX_COMABLE, ARG
#DIM CLOTH_NO_INNER_L
#DIM CLOTH_OUTER_PER_L
#DIM CLOTH_OUTER_DEF_L
#DIM CLOTH_INNER_PER_L
#DIM CLOTH_INNER_DEF_L
;로컬 변수로 값을 옮겨 참조한다
CLOTH_NO_INNER_L = CLOTH_NO_INNER
CLOTH_OUTER_PER_L = CLOTH_OUTER_PER
CLOTH_OUTER_DEF_L = CLOTH_OUTER_DEF
CLOTH_INNER_PER_L = CLOTH_INNER_PER
CLOTH_INNER_DEF_L = CLOTH_INNER_DEF
IF ENEMY_TYPE_CHECK_F("MOB") == 1 || ENEMY_TYPE_CHECK_F("CITIZEN") == 1
	;추가 고문 부위 지정
	TRYCCALLFORM SEXCOM_OPTION_MOB_{FLAG:11}_COM{ARG}
		LOCAL = RESULT
	CATCH
		LOCAL = -1
	ENDCATCH
ELSE
	LOCAL = 0
ENDIF
;특정 의상에 의한 보호 기능 무시
;속옷：성기 가리개
IF CFLAG:42 == 398 && CLOTH_NO_INNER_L == 0
	;Ａ공격（강）, Ａ공격（ＳＰ）, Ａ확장 공격
	SIF ARG == 5 || ARG == 1002 || ARG == 1010
		CLOTH_INNER_DEF_L = 100
ENDIF
TFLAG:10 = ARG
SELECTCASE ARG
	;Ｃ공격（약）
	CASE 0
		CALL SEX_COM0, FLAG:11, LOCAL
	;Ｃ공격（강）
	CASE 1
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM1, FLAG:11, LOCAL
		ENDIF
	;Ｖ공격（약）
	CASE 2
		;오토코노코인 경우에는 Ａ공격（약）으로 대체한다
		IF TALENT:오토코노코 > 0
			TFLAG:10 = 0
			CALL SEX_COM4, FLAG:11, LOCAL
		;남자인 경우에는 Ｃ공격（약）으로 대체한다
		ELSEIF ISMALE()
			TFLAG:10 = 0
			CALL SEX_COM0, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM2, FLAG:11, LOCAL
		ENDIF
	;Ｖ공격（강）
	CASE 3
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			;오토코노코인 경우에는 Ａ공격（강）으로 대체한다
			IF TALENT:오토코노코 > 0
				TFLAG:10 = 1
				CALL SEX_COM5, FLAG:11, LOCAL
			;남자인 경우에는 Ｃ공격（강）으로 대체한다
			ELSEIF ISMALE()
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
			;성처녀는 Ｂ공격으로 하고 싶다(지금은 Ｖ공격（약）으로 돌리고 있다)
			ELSEIF CHECK_HOLYVIRGIN_F()==1
			;	CALL SEX_COM7, FLAG:11, LOCAL
				TFLAG:10 = 2
				CALL SEX_COM2, FLAG:11, LOCAL
			ELSE
				CALL SEX_COM3, FLAG:11, LOCAL
			ENDIF
		ENDIF
	;Ａ공격（약）
	CASE 4
		;남자임과 동시에 양성평등 OFF라면 Ｃ공격（약）으로 대체한다
		IF ISHOLE() == 0
			TFLAG:10 = 0
			CALL SEX_COM0, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM4, FLAG:11, LOCAL
		ENDIF
	;Ａ공격（강）
	CASE 5
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			;남자임과 동시에 양성평등 OFF라면 Ｃ공격（강）으로 대체한다
			IF ISHOLE() == 0
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
			ELSE
				CALL SEX_COM5, FLAG:11, LOCAL
			ENDIF
		ENDIF
	;Ｂ공격（약）
	CASE 6
		CALL SEX_COM6, FLAG:11, LOCAL
	;Ｂ공격（강）
	CASE 7
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			;남자임과 동시에 양성평등 OFF라면 Ｃ공격（약）으로 대체한다
				IF ISHOLE() == 0
				TFLAG:10 = 0
				CALL SEX_COM0, FLAG:11, LOCAL
			ELSE
				CALL SEX_COM7, FLAG:11, LOCAL
			ENDIF
		ENDIF
	;스팽킹
	CASE 8
		CALL SEX_COM8, FLAG:11, LOCAL
	;바늘
	CASE 9
		;잔혹 필터 OFF라면 스팽킹으로 대체한다
		IF CONFIG_CHECK_MANIAC_F(14) == 0
			TFLAG:10 = 8
			CALL SEX_COM8, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM9, FLAG:11, LOCAL
		ENDIF
	;수음
	CASE 10
		;기절 중에는 Ｃ공격（약）으로 대체한다
		IF (TCVARn:12 & 기절)
			TFLAG:10 = 0
			CALL SEX_COM0, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM10, FLAG:11, LOCAL
		ENDIF
	;펠라치오
	CASE 11
		;남자임과 동시에 양성평등 OFF라면 스팽킹으로 대체한다
		IF ISHOLE() == 0
			TFLAG:10 = 8
			CALL SEX_COM8, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM11, FLAG:11, LOCAL
		ENDIF
	;이라마치오
	CASE 12
		;남자임과 동시에 양성평등 OFF라면 스팽킹으로 대체한다
		IF ISHOLE() == 0
			TFLAG:10 = 8
			CALL SEX_COM8, FLAG:11, LOCAL
		;잔혹 필터 OFF라면 펠라치오로 대체한다
		ELSEIF CONFIG_CHECK_MANIAC_F(14) == 0
			TFLAG:10 = 11
			CALL SEX_COM11, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM12, FLAG:11, LOCAL
		ENDIF
	;절정 금지
	CASE 13
		CALL SEX_COM13, FLAG:11, LOCAL
	;의상을 찢는다
	CASE 14
		IF (CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L) && RAND:100 < 70
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSEIF (CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > 0) && RAND:100 < 15
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			RETURN 0
		ENDIF
	;교배 피스톤
	CASE 15
		;오토코노코인 경우에는 애널 피스톤으로 대체한다
		IF TALENT:오토코노코 > 0
			;강구속을 지운다
			SIF (TCVARn:12 & 강구속)
				TCVARn:12 -= 강구속
			TFLAG:10 = 1
			CALL SEX_COM17, FLAG:11, LOCAL
		;남자인 경우에는 Ｃ공격（강）으로 대체한다
		ELSEIF ISMALE()
			;강구속을 지운다
			SIF (TCVARn:12 & 강구속)
				TCVARn:12 -= 강구속
			TFLAG:10 = 1
			CALL SEX_COM1, FLAG:11, LOCAL
		;성처녀는 A피스톤으로 돌릴 예정(현재 상황은 Ｃ공격（강）)
		ELSEIF CHECK_HOLYVIRGIN_F()==1
		;	CALL SEX_COM17, FLAG:11, LOCAL
			;강구속을 지운다
			SIF (TCVARn:12 & 강구속)
				TCVARn:12 -= 강구속
			TFLAG:10 = 1
			CALL SEX_COM1, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM15, FLAG:11, LOCAL
		ENDIF
	;교배 피니쉬
	CASE 16
		;오토코노코인 경우에는 애널 피니쉬으로 대체한다
		IF TALENT:오토코노코 > 0
			;강구속을 지운다
			SIF (TCVARn:12 & 강구속)
				TCVARn:12 -= 강구속
			TFLAG:10 = 1
			CALL SEX_COM18, FLAG:11, LOCAL
		;남자인 경우에는 Ｃ공격（강）으로 대체한다
		ELSEIF ISMALE()
			;강구속을 지운다
			SIF (TCVARn:12 & 강구속)
				TCVARn:12 -= 강구속
			TFLAG:10 = 1
			CALL SEX_COM1, FLAG:11, LOCAL
		;성처녀는 A피니쉬로 돌릴 예정(현재 상황은 Ｃ공격（강）)
		ELSEIF CHECK_HOLYVIRGIN_F()==1
		;	CALL SEX_COM18, FLAG:11, LOCAL
			;강구속을 지운다
			SIF (TCVARn:12 & 강구속)
				TCVARn:12 -= 강구속
			TFLAG:10 = 1
			CALL SEX_COM1, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM16, FLAG:11, LOCAL
		ENDIF
	;애널 피스톤
	CASE 17
		;남자임과 동시에 양성평등 OFF라면 Ｃ공격（강）으로 대체한다
		IF ISHOLE() == 0
			;강구속을 지운다
			SIF (TCVARn:12 & 강구속)
				TCVARn:12 -= 강구속
			TFLAG:10 = 1
			CALL SEX_COM1, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM17, FLAG:11, LOCAL
		ENDIF
	;애널 피니쉬
	CASE 18
		;남자임과 동시에 양성평등 OFF라면 Ｃ공격（강）으로 대체한다
		IF ISHOLE() == 0
			;강구속을 지운다
			SIF (TCVARn:12 & 강구속)
				TCVARn:12 -= 강구속
			TFLAG:10 = 1
			CALL SEX_COM1, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM18, FLAG:11, LOCAL
		ENDIF
	;양 구멍 피스톤
	CASE 19
		;남자의 경우 또는 촉수가 없는 세뇌/흑화 캐릭터의 경우는 이전 턴의 행동에 따라 대체한다
		IF ISMALE() || (FLAG:110 == 1 && TALENT:(FLAG:111):기생 == 0)
			IF TFLAG:20 == 15
				TFLAG:10 = 15
				CALL SEX_COM15, FLAG:11, LOCAL
			ELSEIF TFLAG:20 == 17
				TFLAG:10 = 17
				CALL SEX_COM17, FLAG:11, LOCAL
			ELSE
				;강구속을 지운다
				SIF (TCVARn:12 & 강구속)
					TCVARn:12 -= 강구속
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
			ENDIF
		;성처녀는 A피스톤
		ELSEIF CHECK_HOLYVIRGIN_F()==1
			TFLAG:10 = 17
			CALL SEX_COM17, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM19, FLAG:11, LOCAL
		ENDIF
	;양 구멍 피니쉬
	CASE 20
		;남자의 경우 또는 촉수가 없는 세뇌/흑화 캐릭터의 경우는 이전 턴의 행동에 따라 대체한다
		IF ISMALE() || (FLAG:110 == 1 && TALENT:(FLAG:111):기생 == 0)
			IF TFLAG:20 == 15
				TFLAG:10 = 16
				CALL SEX_COM16, FLAG:11, LOCAL
			ELSEIF TFLAG:20 == 17
				TFLAG:10 = 18
				CALL SEX_COM18, FLAG:11, LOCAL
			ELSE
				;강구속을 지운다
				SIF (TCVARn:12 & 강구속)
					TCVARn:12 -= 강구속
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
			ENDIF
		;성처녀는 A피니쉬
		ELSEIF CHECK_HOLYVIRGIN_F()==1
			TFLAG:10 = 18
			CALL SEX_COM18, FLAG:11, LOCAL
		ELSE
			CALL SEX_COM20, FLAG:11, LOCAL
		ENDIF
	;Ｃ공격Ａ공격（ＳＰ）
	CASE 1000
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L / 2
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			CALL SEX_SPCOM0
		ENDIF
	;Ｖ공격（ＳＰ）
	CASE 1001
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L / 2
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			;오토코노코인 경우에는 Ａ공격（강）으로 대체한다
			IF TALENT:오토코노코 > 0
				TFLAG:10 = 1
				CALL SEX_COM5, FLAG:11, LOCAL
			;남자인 경우에는 Ｃ공격（강）으로 대체한다
			ELSEIF ISMALE()
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
			;성처녀는 Ｃ공격Ａ공격（ＳＰ）으로 돌릴 예정(현재 상황은 Ｃ공격（강）)
			ELSEIF CHECK_HOLYVIRGIN_F()==1
			;	CALL SEX_SPCOM0
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
			ELSE
				CALL SEX_SPCOM1
			ENDIF
		ENDIF
	;Ａ공격（ＳＰ）
	CASE 1002
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L / 2
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			;남자임과 동시에 양성평등 OFF라면 Ｃ공격Ａ공격（ＳＰ）으로 대체한다
				IF ISHOLE() == 0
				TFLAG:10 = 1000
				CALL SEX_SPCOM0
			;소프트 바디 수트의 경우는 관통할 수 없기에 Ａ공격（강）으로 대체한다
			ELSEIF CFLAG:41 == 299 && CFLAG:1 > 0
				TFLAG:10 = 5
				CALL SEX_COM5, FLAG:11, LOCAL
			ELSE
				CALL SEX_SPCOM2
			ENDIF
		ENDIF
	;Ｂ공격（ＳＰ）
	CASE 1003
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			;남자임과 동시에 양성평등 OFF라면 Ｂ공격（약）으로 대체한다
				IF ISHOLE() == 0
				TFLAG:10 = 6
				CALL SEX_COM6, FLAG:11, LOCAL
			ELSE
				CALL SEX_SPCOM3
			ENDIF
		ENDIF
	;전격
	CASE 1004
		CALL SEX_SPCOM4
	;파이즈리
	CASE 1005
		;남자인 경우에는 Ｂ공격（강）으로 대체한다
		IF ISMALE()
			TFLAG:10 = 7
			CALL SEX_COM7, FLAG:11, LOCAL
		ELSE
			CALL SEX_SPCOM5
		ENDIF
	;강제 자위
	CASE 1006
		;기절 중에는 Ｃ공격（약）으로 대체한다
		IF (TCVARn:12 & 기절)
			TFLAG:10 = 0
			CALL SEX_COM0, FLAG:11, LOCAL
		ELSE
			CALL SEX_SPCOM6
		ENDIF
	;수치 플레이
	CASE 1007
		;기절 중에는 Ｃ공격（약）으로 대체한다
		IF (TCVARn:12 & 기절)
			TFLAG:10 = 0
			CALL SEX_COM0, FLAG:11, LOCAL
		;남자임과 동시에 양성평등 OFF라면 Ｃ공격（강）으로 대체한다
		ELSEIF ISHOLE() == 0
			TFLAG:10 = 1
			CALL SEX_COM1, FLAG:11, LOCAL
		ELSE
			CALL SEX_SPCOM7
		ENDIF
	;요도 공격
	CASE 1008
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L / 2
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			IF BASE:Ｃ결계내구력 > 0 || TFLAG:20 < 0
				TFLAG:10 = 1000
				CALL SEX_SPCOM0
			ELSE
				CALL SEX_SPCOM8
			ENDIF
		ENDIF
	;Ｖ확장 공격
	CASE 1009
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L / 2
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			;오토코노코인 경우에는 Ａ공격（강）으로 대체한다
			IF TALENT:오토코노코 > 0
				TFLAG:10 = 1
				CALL SEX_COM5, FLAG:11, LOCAL
			;남자인 경우에는 Ｃ공격（강）으로 대체한다
			ELSEIF ISMALE()
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
			;성처녀는 이라마치오로 돌릴 예정(현재 상황은 Ｃ공격（강）)
			ELSEIF CHECK_HOLYVIRGIN_F()==1
			;	CALL SEX_COM12, FLAG:11, LOCAL
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
			ELSEIF BASE:Ｖ결계내구력 > 0 || TFLAG:20 < 0
				TFLAG:10 = 1001
				CALL SEX_SPCOM1
			;성처녀는 A확장
			ELSEIF CHECK_HOLYVIRGIN_F()==1
				TFLAG:10 = 12
				CALL SEX_COM12, FLAG:11, LOCAL
			ELSE
				CALL SEX_SPCOM9
			ENDIF
		ENDIF
	;Ａ확장 공격
	CASE 1010
		;의상의 내구도가 높으면 의상 파괴를 우선한다
		IF CLOTH_OUTER_PER_L > 90 || CLOTH_INNER_PER_L > CLOTH_INNER_DEF_L / 2
			TFLAG:10 = 14
			CALL SEX_COM14, FLAG:11, LOCAL
		ELSE
			;남자임과 동시에 양성평등 OFF라면 Ｃ공격Ａ공격（ＳＰ）으로 대체한다
				IF ISHOLE() == 0
				TFLAG:10 = 1000
				CALL SEX_SPCOM0
