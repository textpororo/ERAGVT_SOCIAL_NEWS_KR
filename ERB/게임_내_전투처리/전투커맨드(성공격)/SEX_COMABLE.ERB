				TFLAG:10 = 1
				CALL SEX_COM7, FLAG:11, LOCAL
			;남자인 경우에는 Ｃ공격（강）으로 대체한다
			ELSEIF ISMALE()
				TFLAG:10 = 1
				CALL SEX_COM1, FLAG:11, LOCAL
