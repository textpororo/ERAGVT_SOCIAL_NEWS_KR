;양 구멍 피스톤（피스톤 계통으로부터의 파생 한정 행동）

;성 커맨드 분류
@SEX_TYPE_COM19
RETURN 삽입

@SEX_COM19,ARG = 0,ARG:1 = 0
;EX_COM	추가 고문 실행 부위（비트）
;	1：Ｃ　2：Ｖ　4：Ａ　8：Ｂ
;추가 고문
CALL SEX_COMEX_RANDOM, Ｖ+Ａ, ARG:1
EX_COM = RESULT:0
SH_COM = RESULT:1

INSERT |= 질
INSERT |= 애널
;상태 이상：강구속
SIF (TCVARn:12 & 강구속) == 0
	CALL STATE_CHANGE_PKOUSOKU, 100

;흑화 캐릭터의 페니스 위치를 세트
IF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && (ISPENIS(FLAG:111))
	IF RAND:2 == 0 || TCVARn:2 == 자세：Ｖ방어
		TFLAG:18 = 질
	ELSE
		TFLAG:18 = 애널
	ENDIF
ENDIF

;취득 파라미터 계산
;BASE값의 명시적 초기화
REPEAT 12
	LOCAL:COUNT = 0
REND

;방심도 상승
TFLAG:3 += 8

;사정치 600
CALL TENTACLE_SYASEI_UP, 600

;촉수 크기 설정
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 19


;쾌Ｖ
IF ABL:Ｖ감각 == 0
	LOCAL:1 = 30
ELSEIF ABL:Ｖ감각 == 1
	LOCAL:1 = 75
ELSEIF ABL:Ｖ감각 == 2
	LOCAL:1 = 150
ELSEIF ABL:Ｖ감각 == 3
	LOCAL:1 = 300
ELSEIF ABL:Ｖ감각 == 4
	LOCAL:1 = 750
ELSEIF ABL:Ｖ감각 >= 5
	LOCAL:1 = 1500
ENDIF

;쾌Ａ　감각이 낮을 때는 적다
IF CONFIG_CHECK_BALANCE_F(7) == 0
	IF ABL:Ａ감각 == 0
		LOCAL:2 = 6
	ELSEIF ABL:Ａ감각 == 1
		LOCAL:2 = 35
	ELSEIF ABL:Ａ감각 == 2
		LOCAL:2 = 150
	ELSEIF ABL:Ａ감각 == 3
		LOCAL:2 = 300
	ELSEIF ABL:Ａ감각 == 4
		LOCAL:2 = 750
	ELSEIF ABL:Ａ감각 >= 5
		LOCAL:2 = 1500
	ENDIF
;컨피그 ON이라면 쾌Ａ 증가
ELSEIF CONFIG_CHECK_BALANCE_F(7) == 0
	IF ABL:Ａ감각 == 0
		LOCAL:2 = 30
	ELSEIF ABL:Ａ감각 == 1
		LOCAL:2 = 75
	ELSEIF ABL:Ａ감각 == 2
		LOCAL:2 = 150
	ELSEIF ABL:Ａ감각 == 3
		LOCAL:2 = 300
	ELSEIF ABL:Ａ감각 == 4
		LOCAL:2 = 750
	ELSEIF ABL:Ａ감각 >= 5
		LOCAL:2 = 1500
	ENDIF
ENDIF

;메인 부위 절정 예정의 예측
CALL PALAM_VABCestimate(LOCAL,1,2,-1)



IF ENEMY_TYPE_CHECK_F("MOB") == 1
	TRYCALLFORM MESSAGE_MOB_{ARG}_COM19, EX_COM, SH_COM
	;처녀라면 고통이 대량으로 들어간다
	IF TALENT:처녀 > 0 && TCVARn:2 != 자세：Ｖ방어 && BASE:Ｖ결계내구력 <= 0
		LOCAL:10 = 10000
		TALENT:처녀 = -1
		CFLAG:206 = 2
		TCVARn:1 = 1
	ELSE
		LOCAL:10 = 0
	ENDIF
ELSE
	;처녀라면 고통이 대량으로 들어간다
	IF TALENT:처녀 > 0 && TCVARn:2 != 자세：Ｖ방어 && BASE:Ｖ결계내구력 <= 0
		LOCAL:10 = 10000
		CALL LOSTVIRGIN
	ELSE
		LOCAL:10 = 0
	ENDIF
	;지문: 세뇌/흑화 캐릭터가 조작 캐릭터에게 양 구멍 피스톤을 했다
	SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
		CALL MESSAGE_OTHER_SEX_COM19
	;지문: COM17
	CALL MESSAGE_SEX_COM19, EX_COM, SH_COM
ENDIF

;결계에 따른 촉수 개수의 변동
CALL SET_TENTACLE_POOL
;의상에의 피해
CALL CLOTH_BATTLE_DAMAGE, 3

;BASE
EXP:Ｖ경험 += 1
EXP:Ａ경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 0 && INCEST_F(TARGET, FLAG:10 * 100 + FLAG:11, 1) > 0
	EXP:근친교배경험 += 1
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1 && INCEST_F(TARGET, FLAG:111) > 0
	EXP:근친교배경험 += 1



;공순
IF ABL:순종 <= 2
	LOCAL:5 = 30
ELSEIF ABL:순종 == 3
	LOCAL:5 = 75
ELSEIF ABL:순종 == 4
	LOCAL:5 = 150
ELSEIF ABL:순종 >= 5
	LOCAL:5 = 300
ENDIF

;습득
IF ABL:봉사정신 == 0
	LOCAL:6 = 15
ELSEIF ABL:봉사정신 == 1
	LOCAL:6 = 35
ELSEIF ABL:봉사정신 == 2
	LOCAL:6 = 70
ELSEIF ABL:봉사정신 == 3
	LOCAL:6 = 135
ELSEIF ABL:봉사정신 == 4
	LOCAL:6 = 300
ELSEIF ABL:봉사정신 >= 5
	LOCAL:6 = 600
ENDIF

;굴복　Ａ감각에 의존
IF ABL:Ａ감각 == 0
	LOCAL:8 = 20
ELSEIF ABL:Ａ감각 == 1
	LOCAL:8 = 40
ELSEIF ABL:Ａ감각 == 2
	LOCAL:8 = 100
ELSEIF ABL:Ａ감각 == 3
	LOCAL:8 = 200
ELSEIF ABL:Ａ감각 == 4
	LOCAL:8 = 400
ELSEIF ABL:Ａ감각 >= 5
	LOCAL:8 = 900 + ABL:Ａ감각 * 20
ENDIF

;윤활이 낮으면 고통이 들어간다
IF PALAM:윤활 < 1000
	LOCAL:10 += 1000
ELSEIF PALAM:윤활 < 2000
	LOCAL:10 += 200
ENDIF

;Ｖ감각이 낮으면 공포가 들어간다
IF ABL:Ｖ감각 == 0
	LOCAL:11 = 300
ELSEIF ABL:Ｖ감각 == 1
	LOCAL:11 = 75
ELSEIF ABL:Ｖ감각 == 2
	LOCAL:11 = 30
ELSEIF ABL:Ｖ감각 == 3
	LOCAL:11 = 15
ELSE
ENDIF
;Ａ감각이 낮으면 공포가 들어간다
IF ABL:Ａ감각 == 0
	LOCAL:11 = 150
ELSEIF ABL:Ａ감각 == 1
	LOCAL:11 = 75
ELSEIF ABL:Ａ감각 == 2
	LOCAL:11 = 35
ELSEIF ABL:Ａ감각 == 3
	LOCAL:11 = 15
ELSE
ENDIF

;추가 고문 실행
IF EX_COM
	CALL SEX_COMEX, Ｖ+Ａ, 1, EX_COM | SH_COM
	REPEAT 12
		LOCAL:COUNT += RESULT:COUNT
	REND
ENDIF

;대량 사정 가능하면 피니쉬로 이행
IF FLAG:15 >= FLAG:14 * 2
	TFLAG:17 = 20
;그 이외라면 양 구멍 피스톤 속행
ELSE
	TFLAG:17 = 19
ENDIF
;촉수의 행동을 기록
TFLAG:20 = 19

;습득 기본값
LOCAL:6 += 350

;욕정 기본값
LOCAL:7 += 200

;굴복 기본값
LOCAL:8 += 500

;치정 기본값
LOCAL:9 += 300

;고통 기본값
LOCAL:10 += 150

;공포 기본값
LOCAL:11 += 200

;체력 소비 기본값
LOCAL:12 += 5

;계산으로
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
