;바늘

;성 커맨드 분류
@SEX_TYPE_COM9
RETURN 통상

@SEX_COM9,ARG = 0,ARG:1 = 0
;추가 고문
EX_COM = 0
SH_COM = 0

;취득 파라미터 계산
;BASE값의 명시적 초기화
REPEAT 12
	LOCAL:COUNT = 0
REND

;방심도 상승
TFLAG:3 += 25

;사정치 25
CALL TENTACLE_SYASEI_UP, 25

;촉수 크기 설정
CALL SET_TENTACLE_SIZE, FLAG:10, FLAG:11, ENEMY_TYPE_CHECK_F("AKUOTI"), FLAG:111, 9

;메인 부위 절정 예정의 예측
CALL PALAM_VABCestimate(LOCAL,-1)


IF ENEMY_TYPE_CHECK_F("MOB") == 1
	TRYCALLFORM MESSAGE_MOB_{ARG}_COM9, EX_COM, SH_COM
ELSE
	;지문: 세뇌/흑화 캐릭터가 조작 캐릭터에게 바늘을 꽂았다
	SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
		CALL MESSAGE_OTHER_SEX_COM9
	;지문: COM9
	CALL MESSAGE_SEX_COM9
ENDIF

;결계에 따른 촉수 개수의 변동
CALL SET_TENTACLE_POOL
;의상에의 피해
CALL CLOTH_BATTLE_DAMAGE, 10

;BASE
SIF ABL:마조끼
	EXP:고통쾌락경험 += 1

;굴복　마조끼로 변화
IF ABL:마조끼 == 0
	LOCAL:8 = 200
	LOCAL:11 = 10000
ELSEIF ABL:마조끼 == 1
	LOCAL:8 = 400
	LOCAL:11 = 4000
ELSEIF ABL:마조끼 == 2
	LOCAL:8 = 1000
	LOCAL:11 = 2000
ELSEIF ABL:마조끼 == 3
	LOCAL:8 = 2000
	LOCAL:11 = 1000
ELSEIF ABL:마조끼 == 4
	LOCAL:8 = 4000
	LOCAL:11 = 400
ELSEIF ABL:마조끼 >= 5
	LOCAL:11 = 200
	LOCAL:8 = 10000
ENDIF

;공포　대충 순종과 마조끼로
IF ABL:순종 < 3
	IF ABL:마조끼 < 3
		LOCAL:11 = 5000
	ELSE
		LOCAL:11 = 500
	ENDIF
ELSE
	IF ABL:마조끼 < 3
		LOCAL:11 = 500
	ELSE
		LOCAL:11 = 50
	ENDIF
ENDIF

;행동 예약 없음
TFLAG:17 = -1
;촉수의 행동을 기록
TFLAG:20 = 9

;습득 기본값
LOCAL:6 += 0

;욕정 기본값
LOCAL:7 += 0

;굴복 기본값
LOCAL:8 += 500

;치정 기본값
LOCAL:9 += 0

;고통 기본값
LOCAL:10 += 5000

;공포 기본값
LOCAL:11 += 2500

;체력 소비 기본값
LOCAL:12 += 300

;계산으로
CALL PALAM_CAL, LOCAL:0, LOCAL:1, LOCAL:2, LOCAL:3, LOCAL:4, LOCAL:5, LOCAL:6, LOCAL:7, LOCAL:8, LOCAL:9, LOCAL:10, LOCAL:11, LOCAL:12
