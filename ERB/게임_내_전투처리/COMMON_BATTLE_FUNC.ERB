IF (TALENT:임신 == 4 || TALENT:임신 == 5) && CFLAG:222 >= 56
	LOCAL = RAND:100
	IF LOCAL < 3 + SQRT(CFLAG:222 / 4)
		;공중에 있을 경우 착지한다
		SIF (TCVARn:216 & 1)
			TCVARn:216 = 4
		;FEAT 효과에 따른 착지 시의 반동
		SIF TALENT:날개 > 0
			TFLAG:99 += 1
		;거리 표시
