;--------------------------------------------------
;민첩의 차이에 따른 보정
@CORRECTION_BINSYOU, ARG
IF ARG < 10
	LOCAL = ARG * 15 / 100
ELSEIF ARG < 25
	LOCAL = ARG * 38 / 100
ELSEIF ARG < 45
	LOCAL = ARG * 75 / 100
ELSEIF ARG >= 75
	LOCAL = 64 + ARG * 15 / 100
ELSE
	LOCAL = ARG
ENDIF
RETURN LOCAL

;식중함수판
@CORRECTION_BINSYOU_F(ARG)
#FUNCTION
IF ARG < 10
	LOCAL = ARG * 15 / 100
ELSEIF ARG < 25
	LOCAL = ARG * 38 / 100
ELSEIF ARG < 45
	LOCAL = ARG * 75 / 100
ELSEIF ARG >= 75
	LOCAL = 64 + ARG * 15 / 100
ELSE
	LOCAL = ARG
ENDIF
RETURNF LOCAL

;--------------------------------------------------
;변신 능력이 있는 캐릭터 보정
@CORRECTION_TRANS(ARG)
#FUNCTION
IF (TALENT:변신능력 == 1 && CFLAG:1 == 0) || TALENT:변신능력 == -1
	ARG = ARG * CFLAG:10 / 100
ELSEIF TALENT:변신능력 == 1 && CFLAG:1 == 2
	;ＳＰ변신의 능력 배율은 100 미만이 되지 않는다
	LOCAL = CFLAG:11
	SIF LOCAL < 100
		LOCAL = 100
	ARG = ARG * LOCAL / 100
ENDIF
RETURNF ARG

