ELSEIF ARG:0 < 5000
	LOCAL += 150 + RAND:30
ELSEIF ARG:0 < 10000
	LOCAL += 200 + RAND:40
ELSEIF ARG:0 < 20000
	LOCAL += 250 + RAND:60
ELSE
	LOCAL += 300 + RAND:80
ENDIF
IF ARG:1 == 0
ELSEIF ARG:1 < 100
	LOCAL += 25 + RAND:5
ELSEIF ARG:1 < 1000
	LOCAL += 50 + RAND:10
ELSEIF ARG:1 < 5000
	LOCAL += 75 + RAND:15
ELSEIF ARG:1 < 10000
	LOCAL += 100 + RAND:20
ELSEIF ARG:1 < 20000
	LOCAL += 125 + RAND:30
ELSE
	LOCAL += 150 + RAND:40
ENDIF
IF ARG:2 == 0
ELSEIF ARG:1 < 100
	LOCAL += 25 + RAND:5
ELSEIF ARG:1 < 1000
	LOCAL += 50 + RAND:10
ELSEIF ARG:1 < 5000
	LOCAL += 75 + RAND:15
ELSEIF ARG:1 < 10000
	LOCAL += 100 + RAND:20
ELSEIF ARG:1 < 20000
	LOCAL += 125 + RAND:30
ELSE
	LOCAL += 150 + RAND:40
ENDIF

