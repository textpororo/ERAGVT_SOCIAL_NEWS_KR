	LOCAL += 100
	;사정에 의해 보정
    IF CFLAG:650 > 0 && CFLAG:651 < 1
    	LOCAL += 5 * (CFLAG:650)
	ELSEIF CFLAG:650 > 0 && CFLAG:651 > 0
    	LOCAL += 10 * (CFLAG:650)
	ENDIF
