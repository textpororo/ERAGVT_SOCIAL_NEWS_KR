;--------------------------------------------------
;변신한다
@COM_ABLE0

;구속 중에는 불가능
SIF TCVARn:0 == 0
	RETURN 0
;변신 소질이 없으면 불가능
SIF TALENT:변신능력 != 1
	RETURN 0
;변신 중에는 불가능
SIF CFLAG:1 != 0
	RETURN 0
;임신 발각 후에는 ＴＳ남성화할 수 없다
SIF CHARATALENT_F(TARGET,0,"남자") == 0 && CHARATALENT_F(TARGET,1,"남자") > 0 && CHECK_PREGNANT_F(TARGET) > 0
	RETURN 0
;기력이 10% 미만이면 불가능
SIF BASE:기력*100/MAXBASE:기력 < 10
	RETURN 0
;기절 중에는 불가능
SIF (TCVARn:12 & 기절)
	RETURN 0
;변신 불가 전투 중에는 불가능
SIF GETBATTLESITUATION("변신 불가")==1
	RETURN 0
;공격 불가 전투 중에는 불가능
SIF GETBATTLESITUATION("공격 불가")==1 || (GETBATTLESITUATION("정체 발각 불가") && FLAG:70+FLAG:71 > 0)
	RETURN 0
;자기 부담(自前)으로 표시하는 경우는 일람에 비표시(이하 모든 커맨드에 대해 공통)
SIF TCVARn:8 < 10
	RETURN 0
RETURN 1



