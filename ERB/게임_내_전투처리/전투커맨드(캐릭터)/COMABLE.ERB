	LOCAL = -2
;스마타 애태우기는 실행 난도 3
ELSEIF ARG == 103
	LOCAL = -3
;그 이외는 실행 난도 1
ELSE
	LOCAL = -1
ENDIF

;성저항
SIF (MESSAGE_BRANCH_F(TARGET) & 성저항) > 0
	LOCAL += 1
;윤활+공순+욕정이 3000 이상
SIF PALAM:윤활 + PALAM:공순 + PALAM:욕정 >= 3000
	LOCAL += 1
;시간 경과
LOCAL += SQRT(TFLAG:0 + 1)

SIF LOCAL > 0
	RETURNF 1
;그 외는 사용 불가
RETURNF 0











