;수음공격
@COM100

;거리 표시
CALL PRINT_DISTANCE
PRINTL 

;사정 부위 플래그:손을 세운다
SIF (FLAG:15 >= FLAG:14 * 2) || RAND:2 == 0
	TFLAG:4 = TFLAG:4 | 손
;강구속 중에는 사정 부위 플래그를 접는다
SIF (TCVARn:12 & 강구속)
	TFLAG:4 = 0

;방어 플래그：봉사한다로 한다
TCVARn:2 = 자세：봉사한다

;방심도 상승
TFLAG:3 += 55 + 5 * ABL:기교

;지문: 수음 공격
CALL MESSAGE_BATTLE_CHARA_SEX_ATTACK100
;지문: 세뇌/흑화 캐릭터가 조작 캐릭터의「수음공격」을 봤다
SIF ENEMY_TYPE_CHECK_F("AKUOTI") == 1
	CALL MESSAGE_OTHER_BATTLE_CHARA_SEX_ATTACK100

;사정 게이지를 기록
LOCAL:1 = FLAG:15

;사정
LOCAL = MIN(MAXBASE:성내성 / 5, 75) + 100 + ABL:기교 * 25
CALL TENTACLE_SYASEI_UP, LOCAL

PRINTL
IF (FLAG:110 == 0)
	CALL TENTACLE_ACCESS, "NAME"
ELSEIF (FLAG:110 == 1)
	PRINTFORM %PRINT_TRANSCALLNAME(FLAG:111)%
ENDIF
PRINT 에게 
FONTBOLD
PRINTFORM {FLAG:15 - LOCAL:1} 의 쾌락
FONTREGULAR
PRINTL 을 줬다！
PRINTW 

;촉수의 사정 체크
CALL TENTACLE_SYASEI_CHECK
PRINTL 

;기교Lv3 이상이면 10% 확률로 펠라치오를 유발한다
IF ABL:기교 >= 3 && RAND:100 < 10
	IF ENEMY_TYPE_CHECK_F("MOB") == 1 || ENEMY_TYPE_CHECK_F("CITIZEN") == 1
		TRYCALLFORM TENTACLE_MOB_{FLAG:11}_REACTION_REF, 3
	ELSEIF ENEMY_TYPE_CHECK_F("LASTBOSS") >= 1
		TRYCALLFORM TENTACLE_LASTBOSS_{FLAG:11}_REACTION_REF, 3
	ELSE
		TRYCALLFORM TENTACLE_BOSS_{FLAG:11}_REACTION_REF, 3
	ENDIF
	SIF RESULT >= 0
		TFLAG:17 = RESULT
ENDIF

;습득 획득
COMMON_PALAM:6 += 450 + 25 * (ABL:순종 + ABL:봉사정신 * 2)

;행동 포인트를 +1
EX:행동포인트 += 1

RETURN 1
