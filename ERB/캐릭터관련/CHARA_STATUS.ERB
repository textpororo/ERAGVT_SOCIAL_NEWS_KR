;페이지 전이 때마다 스택이 쌓여 버려…

;--------------------------------------------------
;스테이터스 표시 베이스
@SHOW_STATUS_CHARA_SELECT ,ARG
#DIM LCOUNT
#DIM PAGE=1

;page4
	#DIM PAPA
	#DIM MAMA
;page5
	#DIM B_SIZE
	#DIM W_SIZE
	#DIM H_SIZE


REDRAW 0
$REPRINT
	CALL LB
	DRAWLINE
SELECTCASE PAGE
CASE 1
	PRINTL 　　　　　　　　　　　　　　　　　기본 스테이터스 정보　　　　　　　　　　　　　PAGE(1/5)
	DRAWLINE
	;통상 스테이터스 표시
	CALL SHOW_STATUS_BASE_DISPSTATUS, ARG
	CALL SHORTLINE
	CALL SHOW_STATUS_BASE_STATUSINFO, ARG
	CALL SHOW_STATUS_SENGI, ARG
	IF TALENT:ARG:C결계 + TALENT:ARG:V결계 + TALENT:ARG:A결계 + TALENT:ARG:B결계 + TALENT:ARG:피임결계
		PRINTL 
		CALL SHORTLINE
	ENDIF
	CALL SHOW_STATUS_SHIELD, ARG
	IF CFLAG:ARG:32 > 0
		PRINTL 
		CALL SHORTLINE
	ENDIF
	CALL SHOW_STATUS_TATTOO, ARG
	PRINTL 
	CALL SHORTLINE
	CALL SHOW_STATUS_CLOTH, ARG
	DRAWLINE
	PRINTL
	PRINT  [1]  [앞 페이지]　　
	SETCOLORBYNAME CYAN
	PRINT [1000] 전투　
	RESETCOLOR
	PRINT [2000] 소질　
	PRINT [3000] 무기　
	PRINT [4000] 상관　
	PRINT [5000] 개인　
	PRINTL 　[2]  [뒷 페이지]

	PRINT  [999] 돌아간다　　　　　
	SIF FLAG:700 > 0
		SETCOLOR 128,128,128
		PRINT 　[100] 이전 캐릭터　　　　　　　　　　
		PRINT [200] 다음 캐릭터
	PRINTL 
	RESETCOLOR
	INPUT
	IF RESULT == 999
		RETURN 999
	ELSEIF RESULT == 0
		IF CFLAG:ARG:6 == 0
			CFLAG:ARG:6 = 1
		ELSEIF CFLAG:ARG:6 == 1
			CFLAG:ARG:6 = 2
		ELSEIF CFLAG:ARG:6 == 2
			CFLAG:ARG:6 = 3
			TALENT:ARG:주관시점 = 1
		ELSEIF CFLAG:ARG:6 == 3
			CFLAG:ARG:6 = 4
			TALENT:ARG:주관시점 = 0
		ELSEIF CFLAG:ARG:6 == 4
			CFLAG:ARG:6 = -2
		ELSEIF CFLAG:ARG:6 == NO:ARG
			CFLAG:ARG:6 = 0
		ELSE
			CFLAG:ARG:6 = NO:ARG
		ENDIF
		GOTO REPRINT
	ELSEIF RESULT == 1
		PAGE=5
	ELSEIF RESULT == 2
		PAGE=2
	ELSEIF INRANGE(RESULT,1000,5000)
		PAGE=RESULT/1000
		GOTO REPRINT
	ELSEIF RESULT == 10
		IF TALENT:ARG:주관시점 == 1
			PRINTL 현재『주관 모드』를 사용 중입니다.
			PRINTL [0] 변경하지 않는다
			PRINTL [1] 주관 모드를 종료한다
			INPUT
			IF RESULT == 1
				TALENT:ARG:주관시점 = 0
				PRINTW 주관 모드를 해제했습니다.
			ENDIF
		ELSE
			PRINTL ※지문을 보다 주관적인 묘사로 변경합니다.
			PRINTL 　이 설정은 개별적으로 적용됩니다.
			PRINTL [0] 사용하지 않는다
			PRINTL [1] 사용한다
			INPUT
			IF RESULT == 1
				TALENT:ARG:주관시점 = 1
				PRINTW 지문에 대해『주관 모드』를 활성화합니다.
			ENDIF
		ENDIF
		GOTO REPRINT
	ELSEIF RESULT == 11 && FLAG:700 == 0
		;캐릭터의 호칭 설정
		CALL FIRSTSETTING_CHARA_CALLNAME, ARG
		GOTO REPRINT
	ELSEIF RESULT == 12 && FLAG:700 == 0
		CALL FIRSTSETTING_CHARA_SELFCALL, ARG
		GOTO REPRINT
	ELSEIF RESULT == 13 && TALENT:ARG:변신능력 == 1 && FLAG:700 == 0
		LCOUNT = LINECOUNT
		$LOOP_12
		PRINTFORML 변경할 항목을 선택해 주십시오.
		CALL SHORTLINE
		PRINTFORML [0]　변신 후 이름・・・・・\@(CFLAG:ARG:2 == 1) ?・『%CSTR:ARG:0%』#・ 없음\@
		PRINTFORML [1]　변신 후 통칭・・・・・\@(CFLAG:ARG:3 == 1) ?・『%CSTR:ARG:1%』#・ 없음\@
		PRINTFORML [2]　변신 시 구호・・・・・\@(CFLAG:ARG:4 == 1) ?・『%CSTR:ARG:2%』#・ 없음\@
		PRINTFORML [3]　변신 후 자칭 구상・・\@(CFLAG:ARG:5 == 1) ?・『%CSTR:ARG:3%』#・ 없음\@
		PRINTL 
		PRINTFORML [99] 돌아간다
		$INPUT_LOOP_12
		INPUT
		IF RESULT == 0
			;캐릭터의 변신 후 이름 설정
			CALL FIRSTSETTING_CHARA_TRANSAFTERNAME, ARG
		ELSEIF RESULT == 1
			;캐릭터 변신 후 통칭 설정
			IF CFLAG:ARG:2 == 1
				;캐릭터 변신 후 통칭 플래그를 세운다
				CFLAG:ARG:3 = 1
				CALL FIRSTSETTING_CHARA_TRANSAFTERCALLNAME, ARG
			ENDIF
		ELSEIF RESULT == 2
			;캐릭터 변신 시 구호 설정
			CALL FIRSTSETTING_CHARA_TRANSCALL, ARG
		ELSEIF RESULT == 3
			;캐릭터의 자칭 구상을 설정
			CALL FIRSTSETTING_CHARA_NANORI, ARG
		ELSEIF RESULT == 99
			GOTO REPRINT
		ELSE
			GOTO INPUT_LOOP_12
		ENDIF
		CLEARLINE LINECOUNT - LCOUNT
		GOTO LOOP_12
		
	ELSEIF RESULT == 100 && FLAG:700 == 0
		ARG --
		SIF ARG == 0
			ARG=CHARANUM-1
		GOTO REPRINT
	ELSEIF RESULT == 200 && FLAG:700 == 0
		ARG ++
		SIF ARG==CHARANUM
			ARG=1
		GOTO REPRINT
	ELSE
		PRINTL 
		PRINTL 올바른 값을 입력해 주십시오.
		GOTO REPRINT
	ENDIF

CASE 2
	;--------------------------------------------------
	;스테이터스 표시 베이스（2페이지 째）
	PRINTL 　　　　　　　　　　　　　　　　　　소질・성(性)성장　　　　　　　　　　　　　　　　PAGE(2/5)
	DRAWLINE
	PRINTFORML 이름　：%NAME:(ARG:0)%
	PRINTFORML 통칭　：%CALLNAME:(ARG:0)%
	PRINTFORML 일인칭：%SELF_CALL(0, 0, ARG:0)%
	PRINTFORML 레벨　：Lv.{ABL:(ARG:0):레벨} （{JUEL:(ARG:0):경험치}％）
	CALL SHORTLINE
	CALL SHOW_STATUS_TALENT, ARG, 0, 1
	CALL SHORTLINE
	CALL SHOW_STATUS_ABL, ARG
	PRINTL 
	CALL SHORTLINE
	CALL SHOW_STATUS_MARK, ARG
	PRINTL 
	CALL SHORTLINE
	CALL SHOW_STATUS_EXP, ARG
	;구슬 표시는 디버그용
	IF FLAG:999 == 1
		PRINTL 
		CALL SHORTLINE
		CALL SHOW_STATUS_JUEL, ARG
	ENDIF
	PRINTL 
	DRAWLINE
	PRINTPLAIN 　
	PRINTL [0] 캐릭터 데이터의 서두　　
	PRINT  [1] [앞 페이지]　　
	PRINT [1000] 전투　
	SETCOLORBYNAME CYAN
	PRINT [2000] 소질　
	RESETCOLOR
	PRINT [3000] 무기　
	PRINT [4000] 상관　
	PRINT [5000] 개인　
	PRINTL 　[2] [뒷 페이지]

	PRINT  [999] 돌아간다　　　　　
	SIF FLAG:700 > 0
		SETCOLOR 128,128,128
		PRINT 　[100] 이전 캐릭터　　　　　　　　　　
		PRINT [200] 다음 캐릭터
	PRINTL 
	RESETCOLOR
	INPUT
	IF RESULT == 999
		RETURN 999
	ELSEIF RESULT == 0
		TARGET = ARG
		CALL EXPORT_CSV
	ELSEIF RESULT == 1
		PAGE=1
	ELSEIF RESULT == 2
		PAGE=3
	ELSEIF INRANGE(RESULT,1000,5000)
		PAGE=RESULT/1000
		GOTO REPRINT
	ELSEIF RESULT == 100 && FLAG:700 == 0
		ARG --
		SIF ARG == 0
			ARG=CHARANUM-1
		GOTO REPRINT
	ELSEIF RESULT == 200 && FLAG:700 == 0
		ARG ++
		SIF ARG==CHARANUM
			ARG=1
		GOTO REPRINT
	ELSE
		PRINTL 
		PRINTL 올바른 값을 입력해 주십시오.
		GOTO REPRINT
	ENDIF

	;--------------------------------------------------
	;스테이터스 표시의 베이스 (3페이지 째)
CASE 3
	PRINTL 　　　　　　　　　　　　　　　　　 무기 커스터마이즈　　　　　　　　　　　　　　 PAGE(3/5)
	DRAWLINE
	PRINTFORML 이름　：%NAME:(ARG:0)%
	PRINTFORML 통칭　：%CALLNAME:(ARG:0)%
	PRINTFORML 일인칭：%SELF_CALL(0, 0, ARG:0)%
	PRINTFORML 레벨　：Lv.{ABL:(ARG:0):레벨} （{JUEL:(ARG:0):경험치}％）
	CALL SHORTLINE
	CALL SHOW_WEAPON_EDIT,ARG,1
	CALL SHORTLINE
	CALL SHOW_WEAPON_EDIT,ARG,2
	CALL SHORTLINE
	CALL SHOW_WEAPON_EDIT,ARG,3
	PRINTL 
	DRAWLINE
	IF CFLAG:ARG:0 != 상태_건강 || FLAG:700
		SETCOLOR 105,105,105
		PRINTPLAIN [0] 무기 커스터마이즈 화면으로 이동　　　
		RESETCOLOR
	ELSE
		PRINT [0] 무기 커스터마이즈 화면으로 이동　　　
	ENDIF
	PRINTL
	PRINT  [1] [앞 페이지]　　
	PRINT [1000] 전투　
	PRINT [2000] 소질　
	SETCOLORBYNAME CYAN
	PRINT [3000] 무기　
	RESETCOLOR
	PRINT [4000] 상관　
	PRINT [5000] 개인　
	PRINTL 　[2] [뒷 페이지]
	PRINT  [999] 돌아간다　　　　　　

	SIF FLAG:700 > 0
		SETCOLOR 128,128,128
		PRINT [100] 이전 캐릭터　　　　　　　　　　
		PRINT [200] 다음 캐릭터
	PRINTL 
	RESETCOLOR
	INPUT
	IF RESULT == 999
		RETURN 999
	ELSEIF RESULT == 0 && CFLAG:ARG:0 == 상태_건강 && FLAG:700 == 0
		CALL WEAPON_CUSTOMIZE, ARG
		GOTO REPRINT
	ELSEIF RESULT == 1
		PAGE=2
	ELSEIF RESULT == 2
		PAGE=4
	ELSEIF INRANGE(RESULT,1000,5000)
		PAGE=RESULT/1000
		GOTO REPRINT
	ELSEIF RESULT == 100 && FLAG:700 == 0
		ARG --
		SIF ARG == 0
			ARG=CHARANUM-1
		GOTO REPRINT
	ELSEIF RESULT == 200 && FLAG:700 == 0
		ARG ++
		SIF ARG==CHARANUM
			ARG=1
		GOTO REPRINT
	ELSE
		PRINTL 
		PRINTL 올바른 값을 입력해 주십시오.
		GOTO REPRINT
	ENDIF

	;--------------------------------------------------
	;스테이터스 표시 베이스 (4페이지 째)
CASE 4
	PRINTL 　　　　　　　　　　　　　　　　　캐릭터  상관　　　　　　　　　　　　　　　　  PAGE(4/5)
	DRAWLINE
	PRINTFORML 이름　：%NAME:(ARG:0)%
	PRINTFORML 통칭　：%CALLNAME:(ARG:0)%
	PRINTFORML 일인칭：%SELF_CALL(0, 0, ARG:0)%
	PRINTFORML 레벨　：Lv.{ABL:(ARG:0):레벨} （{JUEL:(ARG:0):경험치}％）
	LOCALS = \@ISMALE(ARG:0) ? 남 # 여\@
	PRINTFORML 성별　：%LOCALS%
	CALL SHORTLINE
	LCOUNT = 0
	FOR LOCAL, 0, CHARANUM
		SIF LOCAL == MASTER
			CONTINUE
		SIF LOCAL == ARG:0
			CONTINUE
		IF RELATION:(ARG:0):LOCAL
			CALL GET_RELATION, ARG:0, LOCAL
			PRINTFORML 　({LOCAL, 2,RIGHT})　%NAME:LOCAL%　→　%RESULTS%
			LCOUNT ++
		ENDIF
	NEXT
	IF LCOUNT == 0
		PRINTL 　　  상관관계가 있는 캐릭터：없음
		LCOUNT ++
	ENDIF
	IF CFLAG:(ARG:0):120 > 0 || CFLAG:(ARG:0):121 > 0 || CFLAG:(ARG:0):122 > 0
		PRINTL 
		PRINTL 　　　　　　　인맥：
		LCOUNT += 2
	ENDIF
	;인맥 관계
	IF CFLAG:(ARG:0):120 == 1
		PRINTL 　　　　　　소문을 좋아하는 친구
		LCOUNT ++
	ENDIF
	IF CFLAG:(ARG:0):121 == 1
		PRINTL 　　　　　　경찰 관계자
		LCOUNT ++
	ELSEIF CFLAG:(ARG:0):121 == 2
		PRINTL 　　　　　　경찰 상층부
		LCOUNT ++
	ENDIF
	IF CFLAG:(ARG:0):122 > 0
		PRINT 　　　　　　정보원(
		IF GETBIT(CFLAG:(ARG:0):122, 2) && GETBIT(CFLAG:(ARG:0):122, 3)
			PRINT 무법자
		ELSEIF GETBIT(CFLAG:(ARG:0):122, 2) && GETBIT(CFLAG:(ARG:0):122, 4)
			PRINT 경박한 남자
		ELSEIF GETBIT(CFLAG:(ARG:0):122, 3) && GETBIT(CFLAG:(ARG:0):122, 4)
			PRINT 노신사
		ELSEIF GETBIT(CFLAG:(ARG:0):122, 2)
			PRINT 천박한 남자
		ELSEIF GETBIT(CFLAG:(ARG:0):122, 3)
			PRINT 점잖은 남자
		ELSEIF GETBIT(CFLAG:(ARG:0):122, 4)
			PRINT 탐정풍의 남자
		ELSE
			PRINT 평범한 남자
		ENDIF
		PRINTL )
		LCOUNT ++
	ENDIF
	IF CFLAG:(ARG:0):123 == 1
		PRINTL 　　　　　　유복한 친정
		LCOUNT ++
	ENDIF

	FOR LOCAL, 0, 20 - LCOUNT
		PRINTL 
	NEXT
	IF RELATION:(ARG:0):(ARG:0) > 0
		CALL SHORTLINE
		PRINT 　혈통　(
		SIF GETBIT(RELATION:(ARG:0):(ARG:0), 혈통불명)
			PRINT 　？？？
		SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｃ촉수)
			PRINT 　Ｃ촉수
		SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｖ촉수)
			PRINT 　Ｖ촉수
		SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ａ촉수)
			PRINT 　Ａ촉수
		SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｂ촉수)
			PRINT 　Ｂ촉수
		SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｓ촉수)
			PRINT 　Ｓ촉수
		SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｐ촉수)
			PRINT 　Ｐ촉수
		SIF GETBIT(RELATION:(ARG:0):(ARG:0), Ｈ촉수)
			PRINT 　Ｈ촉수
		PRINTL 　)
	ELSE
		PRINTL 
	ENDIF
	IF CFLAG:(ARG:0):7 || CFLAG:(ARG:0):9
		CALL SHORTLINE
	ELSE
		PRINTL 
	ENDIF
	PAPA = 0
	MAMA = 0
	IF CFLAG:(ARG:0):9 <= -100
		FOR LOCAL, 0, CHARANUM
			SIF LOCAL == MASTER
				CONTINUE
			SIF LOCAL == ARG:0
				CONTINUE
			SIF CFLAG:LOCAL:240 == CFLAG:(ARG:0):9 * -1 - 100
				PAPA = LOCAL
		NEXT
	ENDIF
	IF CFLAG:(ARG:0):7 > 0
		FOR LOCAL, 0, CHARANUM
			SIF LOCAL == MASTER
				CONTINUE
			SIF LOCAL == ARG:0
				CONTINUE
			SIF CFLAG:LOCAL:240 == CFLAG:(ARG:0):7
				MAMA = LOCAL
		NEXT
	ENDIF
	IF PAPA > 0
		PRINTFORM 　친부　:　%NAME:PAPA% 
		SIF CFLAG:PAPA:34 > 0
			PRINTFORM ({BASE:PAPA:실제나이})
		IF GETBIT(RELATION:PAPA:MAMA,부모자식) || GETBIT(RELATION:PAPA:MAMA,형제자매) || GETBIT(RELATION:PAPA:MAMA,조부모손주) || GETBIT(RELATION:PAPA:MAMA,아저씨아줌마) || GETBIT(RELATION:PAPA:MAMA,조카조카딸)
			PRINT 　　(
			IF GETBIT(RELATION:PAPA:MAMA,부모자식)
				IF (BASE:PAPA:실제나이 == BASE:MAMA:실제나이 && CFLAG:PAPA:240 < CFLAG:MAMA:240) || BASE:PAPA:실제나이 > BASE:MAMA:실제나이
					;娘
					PRINT 딸
				ELSEIF (BASE:PAPA:실제나이 == BASE:MAMA:실제나이 && CFLAG:PAPA:240 > CFLAG:MAMA:240) || BASE:PAPA:실제나이 < BASE:MAMA:실제나이
					;母
					PRINT 엄마
				ENDIF
				SIF GETBIT(RELATION:PAPA:MAMA,형제자매) || GETBIT(RELATION:PAPA:MAMA,조부모손주) || GETBIT(RELATION:PAPA:MAMA,아저씨아줌마) || GETBIT(RELATION:PAPA:MAMA,조카조카딸)
					PRINT /
			ENDIF
			IF GETBIT(RELATION:PAPA:MAMA,형제자매)
				IF (BASE:PAPA:실제나이 == BASE:MAMA:실제나이 && CFLAG:PAPA:240 < CFLAG:MAMA:240) || BASE:PAPA:실제나이 > BASE:MAMA:실제나이
					;妹
					PRINT 여동생
				ELSEIF (BASE:PAPA:실제나이 == BASE:MAMA:실제나이 && CFLAG:PAPA:240 > CFLAG:MAMA:240) || BASE:PAPA:실제나이 < BASE:MAMA:실제나이
					;姉
					PRINT 누나
				ENDIF
				SIF GETBIT(RELATION:PAPA:MAMA,조부모손주) || GETBIT(RELATION:PAPA:MAMA,아저씨아줌마) || GETBIT(RELATION:PAPA:MAMA,조카조카딸)
					PRINT /
			ENDIF
			IF GETBIT(RELATION:PAPA:MAMA,조부모손주)
				IF (BASE:PAPA:실제나이 == BASE:MAMA:실제나이 && CFLAG:PAPA:240 < CFLAG:MAMA:240) || BASE:PAPA:실제나이 > BASE:MAMA:실제나이
					;孫, 손자 혹은 손녀
					PRINT 손녀
				ELSEIF (BASE:PAPA:실제나이 == BASE:MAMA:실제나이 && CFLAG:PAPA:240 > CFLAG:MAMA:240) || BASE:PAPA:실제나이 < BASE:MAMA:실제나이
					;祖母 꺄아아아악
					PRINT 할머니
				ENDIF
				SIF GETBIT(RELATION:PAPA:MAMA,아저씨아줌마) || GETBIT(RELATION:PAPA:MAMA,조카조카딸)
					PRINT /
			ENDIF
			IF GETBIT(RELATION:PAPA:MAMA,아저씨아줌마)
					;おば
					PRINT 아줌마
			ELSEIF GETBIT(RELATION:PAPA:MAMA,조카조카딸)
					;姪
					PRINT 조카딸
			ENDIF
			PRINT 와(과)의 근친상간)
		ENDIF
	ELSEIF CFLAG:(ARG:0):9 > 0
		IF CFLAG:(ARG:0):9 < 100
				TRYCALLFORM TENTACLE_BOSS_{CFLAG:(ARG:0):9}_GETNAME
		ELSEIF CFLAG:(ARG:0):9 < 200
				TRYCALLFORM TENTACLE_LASTBOSS_{CFLAG:(ARG:0):9 - 100}_GETNAME
		ELSEIF CFLAG:(ARG:0):9 == 200
				RESULTS = 잡몹 촉수
		ELSE
				TRYCALLFORM TENTACLE_MOB_{CFLAG:(ARG:0):9 - 200}_GETNAME
		ENDIF
		PRINTFORM 　친부　:　%RESULTS%
	ELSEIF CFLAG:(ARG:0):9 == -100
		PRINTFORM 　친부　:　쌩판 남
	ENDIF
	PRINTL 
	IF MAMA > 0
		PRINTFORM 　친모　:　%NAME:MAMA% 
		SIF CFLAG:MAMA:34 > 0
			PRINTFORM ({BASE:MAMA:실제나이})
		IF GETBIT(RELATION:MAMA:PAPA,부모자식) || GETBIT(RELATION:MAMA:PAPA,형제자매) || GETBIT(RELATION:MAMA:PAPA,조부모손주) || GETBIT(RELATION:MAMA:PAPA,아저씨아줌마) || GETBIT(RELATION:MAMA:PAPA,조카조카딸)
			PRINT 　　(
			IF GETBIT(RELATION:MAMA:PAPA,부모자식)
				IF (BASE:PAPA:실제나이 == BASE:MAMA:실제나이 && CFLAG:PAPA:240 > CFLAG:MAMA:240) || BASE:PAPA:실제나이 < BASE:MAMA:실제나이
