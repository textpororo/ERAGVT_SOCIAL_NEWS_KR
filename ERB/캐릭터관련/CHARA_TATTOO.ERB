;---------------------------------------------
;음문의 위치 결정
;C	하복부
;V	복부
;A	오른쪽 엉덩이
;B	왼쪽 가슴

@TATTOO_POSITION
#DIM CAL_VAR, 3
#DIM LCOUNT

VARSET CAL_VAR
;한 군데일 경우, 가장 감각LV이 높은 부위가 된다
;감각LV에는 민감에 1.2배, 둔감에0.8배, '음~' 계 소질에는 1.5배의 보정을 건다
FOR LCOUNT, 0, 감각수
	CAL_VAR:0 = TATTOO_LV_CAL(LCOUNT)
	IF CAL_VAR:0 > CAL_VAR:1
		CAL_VAR:1 = CAL_VAR:0
		CAL_VAR:2 = LCOUNT
	ENDIF
NEXT
SIF CAL_VAR:1 == TATTOO_LV_CAL(1)
	CAL_VAR:2 = 1

RETURN CAL_VAR:2



;---------------------------------------------
;음문용 감각LV 보정
;ARG	감각 지정

@TATTOO_LV_CAL(ARG)
#FUNCTION
#DIM CAL_VAR

CAL_VAR = ABL:ARG * (TALENT:(ARG * 2 + 100) + 5) / 5 * (5 - TALENT:(ARG * 2 + 101)) / 5 * (TALENT:(ARG + 153) + 2) / 2

RETURNF CAL_VAR



;---------------------------------------------
;음문의 보존 처리
;ARG	진행도
@SAVE_TATTOO, ARG
#DIM LCOUNT

SIF TALENT:촉수의포로 == 0
	RETURN 0

;진행도는 오염도(얼마나 함락에 가까워지고 있는지 나타내는 내부값)와 연동된다
ARG = LIMIT(ARG, 0, 100)
;오염도의 연동을 끊고 있는 경우, 5~10% 진행한다
IF CONFIG_CHECK_MANIAC_F(10) == 0 && CFLAG:0 == 1
	ARG = CFLAG:32 / 10000000000000000
	ARG = LIMIT(ARG + 5 + RAND:6,0,100)
ENDIF
IF CFLAG:32 == 0
	CFLAG:32 = ARG * 10000000000000000
	;음문 복잡화 플래그 체크
	LOCAL = CONFIG_CHECK_MANIAC_F(8)
	;복수 장소 플래그 체크
	;'음~' 계 소질이 없으면 추가로 음문이 붙지 않는다
	IF CONFIG_CHECK_MANIAC_F(7) == 1 && (TALENT:음핵 + TALENT:음호 + TALENT:음고 + TALENT:음유) > 0
		FOR LCOUNT, 0, 감각수
			SIF TALENT:(LCOUNT + 153)
				CFLAG:32 += (LOCAL * (RAND:19 + 1) + 1) * POWER(100, LCOUNT)
		NEXT
	ELSE
		CALL TATTOO_POSITION
		CFLAG:32 += ((LOCAL)* (RAND:19 + 1) + 1) * POWER(100, RESULT)
	ENDIF
ELSE
	CFLAG:32 = CFLAG:32 % 10000000000000000
	CFLAG:32 += ARG * 10000000000000000
	;복수 장소 플래그 체크
	;'음~' 계 소질이 없으면 추가로 음문이 붙지 않는다
	IF CONFIG_CHECK_MANIAC_F(7) == 1 && (TALENT:음핵 + TALENT:음호 + TALENT:음고 + TALENT:음유) > 0
		CALL TATTOO_ACCESS, "POSITION_BIT"
		LOCAL = RESULT
		;신규로 음문이 나오는 부위가 없는지를 체크
		FOR LCOUNT, 0, 감각수
			IF GETBIT(LOCAL, LCOUNT) == 0 && TALENT:(LCOUNT + 153)
				;음문 복잡화 플래그 체크
				CFLAG:32 += ((CONFIG_CHECK_MANIAC_F(8)) * (RAND:19 + 1) + 1) * POWER(100, LCOUNT)
			ENDIF
		NEXT
	ENDIF
ENDIF

;---------------------------------------------
;추가 음문의 보존 처리
;음문 번호 4
;0:이마　1:목구멍(후하두, 喉下部)　2:등　3:오른쪽 뺨　4:왼쪽 뺨　5:오른쪽 허벅지(우대퇴부)　6:왼쪽 허벅지(좌대퇴부)

@SAVE_TATTOO_ADDITIONAL, ARG
#DIM BIT_VAR
#DIM RAND_VAR
#DIM LCOUNT

VARSET BIT_VAR
;'음~' 계 소질을 세고, 모두 없으면 종료
FOR LCOUNT, 0, 감각수
	SIF TALENT:(LCOUNT + 153)
		BIT_VAR += 1
NEXT

SIF BIT_VAR < 감각수
	RETURN 0

;음문이 몇 개 붙어있는지 알아보고, 5개 이상이면 종료
CALL TATTOO_ACCESS, "TATTOO_TYPE", 7
BIT_VAR = RESULT
SIF BITNUM(BIT_VAR) >= 5
	RETURN 0

;새겨지는 음문의 중복 체크
DO
	RAND_VAR = RAND:7
	;이미 있는 경우는 불가능
	SIF BIT_VAR & POWER(2, RAND_VAR)
		RAND_VAR = -1

	;이미 왼쪽에 있는 경우는 불가능
	SIF RAND_VAR > 2 && RAND_VAR % 2 && BIT_VAR & POWER(2, RAND_VAR + 1)
		RAND_VAR = -1

	;이미 오른쪽에 있는 경우는 불가능
	SIF RAND_VAR > 2 && RAND_VAR % 2 == 0 && BIT_VAR & POWER(2, RAND_VAR - 1)
		RAND_VAR = -1
LOOP RAND_VAR < 0

SETBIT BIT_VAR, RAND_VAR
CFLAG:32 = CFLAG:32 % 1000000000000 + CFLAG:32 / 10000000000000000 * 10000000000000000 + BIT_VAR * 100000000000000


;---------------------------------------------
;ARG에 서 있는 비트의 수를 반환하는 식중함수

@BITNUM, ARG
#FUNCTION
#DIM CAL_VAR
#DIM RES_VAR

CAL_VAR = ARG
VARSET RES_VAR
DO
	SIF CAL_VAR % 2
		RES_VAR += 1
	CAL_VAR /= 2
LOOP CAL_VAR > 0

RETURNF RES_VAR


;---------------------------------------------
;음문의 각 파라미터 취득
;ARGS	셀렉터

@TATTOO_ACCESS, ARGS, ARG
#DIM LCOUNT
#DIM CAL_VAR
#DIM PROG_VAR
#DIM DES_VAR, 8

FOR LCOUNT, 0, 8
	DES_VAR:LCOUNT = CFLAG:32 / POWER(100, LCOUNT) % 100
NEXT

VARSET CAL_VAR
SELECTCASE ARGS
	;음문 진행도
	CASE "PROGRESS_VAR"
		RETURN CFLAG:32 / 10000000000000000

	;음문의 감각LV에 따른 랭크
	CASE "TATTOO_RANK"
		IF ARG < 감각수
			RETURN TATTOO_LV_CAL(ARG) * (CFLAG:32 * 5 / 100000000000000000 + 50) / 800
		ELSE
			VARSET CAL_VAR
			FOR LCOUNT, 0, 감각수
				CAL_VAR += TATTOO_LV_CAL(LCOUNT)
			NEXT
			RETURN CAL_VAR * (CFLAG:32 * 5 / 100000000000000000 + 50) / 2000
		ENDIF

	;음문의 추가 문자열의 종류
	CASE "TATTOO_TYPE"
		RETURN DES_VAR:ARG

	;음문의 수
	CASE "POSITION_NUM"
		RETURN (DES_VAR:0 > 0) + (DES_VAR:1 > 0) + (DES_VAR:2 > 0) + (DES_VAR:3 > 0)

	;음문의 위치: 비트
	CASE "POSITION_BIT"
		SIF DES_VAR:0
			CAL_VAR += Ｃ
		SIF DES_VAR:1
			CAL_VAR += Ｖ
		SIF DES_VAR:2
			CAL_VAR += Ａ
		SIF DES_VAR:3
			CAL_VAR += Ｂ

		RETURN CAL_VAR

	;음문의 위치: 문자열
	CASE "POSITION_STR"
		IF (DES_VAR:0 > 0) + (DES_VAR:1 > 0) + (DES_VAR:2 > 0) + (DES_VAR:3 > 0) > 1
			IF DES_VAR:2 == 0 && DES_VAR:3 == 0
				LOCALS = 복부
			ELSEIF DES_VAR:3 == 0
				LOCALS = 하반신
			ELSE
				LOCALS = 체내
			ENDIF
			RESULTS = %LOCALS%
			RETURN 0
		ENDIF

		SIF DES_VAR:0
			LOCALS = 하복부
		SIF DES_VAR:1
			LOCALS = 복부
		SIF DES_VAR:2
			LOCALS = 오른쪽 엉덩이
		SIF DES_VAR:3
			LOCALS = 왼쪽 가슴
		RESULTS = %LOCALS%
		RETURN 0
ENDSELECT


