;２개의 CSTR 스왑
@EXCHANGE_CSTR, ARG, ARG:1, ARG:2
#DIM EX_A
#DIM EX_B
#DIMS BUFF

EX_A = ARG:1
EX_B = ARG:2

BUFF = %CSTR:ARG:EX_A%
CSTR:ARG:EX_A = %CSTR:ARG:EX_B%
CSTR:ARG:EX_B = %BUFF%
RETURN 0
