@MESSAGE_CORRUPT_CHANGE_LOOKS_EYES, ARG
PRINTFORM %PRINT_TRANSCALLNAME(TARGET)%의 目は
IF TALENT:변신능력 == 1
	IF CSTR:TARGET:34 == CSTR:TARGET:35
		CALL SETCOLOR_BY_STR, CSTR:TARGET:34
		PRINTFORM %PRINT_EYECOLORNAME(TARGET, "오른쪽")%の
	ELSE
	;オッドアイ
		CALL SETCOLOR_BY_STR, CSTR:TARGET:34
		PRINTFORM %PRINT_EYECOLORNAME(TARGET, "오른쪽")%
		RESETCOLOR
		PRINTFORM と
		CALL SETCOLOR_BY_STR, CSTR:TARGET:35
		PRINTFORM %PRINT_EYECOLORNAME(TARGET, "왼쪽")%
		RESETCOLOR
		PRINTFORM のオッドアイの
	ENDIF
ELSE
	IF CSTR:TARGET:32 == CSTR:TARGET:33
		CALL SETCOLOR_BY_STR, CSTR:TARGET:32
		PRINTFORM %PRINT_EYECOLORNAME(TARGET, "오른쪽")%の
	ELSE
	;オッドアイ
		CALL SETCOLOR_BY_STR, CSTR:TARGET:32
		PRINTFORM %PRINT_EYECOLORNAME(TARGET, "오른쪽")%
		RESETCOLOR
		PRINTFORM と
		CALL SETCOLOR_BY_STR, CSTR:TARGET:33
		PRINTFORM %PRINT_EYECOLORNAME(TARGET, "왼쪽")%
		RESETCOLOR
		PRINTFORM のオッドアイの
	ENDIF
ENDIF
PRINTFORM %CSTR:TARGET:18%
RESETCOLOR
PRINTFORML に変わった。
PRINTFORML


