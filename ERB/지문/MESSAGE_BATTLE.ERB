;名乗りの외관描写
@MESSAGE_BATTLE_CHARA_NANORI_BYOUSHA
#DIM CUSTOMIZABLE
VARSET LOCAL

;変身구상時の描写
IF EQUIP:600 > 0 || EQUIP:601 > 0 || EQUIP:602 > 0 || EQUIP:603 > 0
	LOCAL:1=EQUIP:600+EQUIP:601+EQUIP:602+EQUIP:603
	IF EQUIP:600 == 1
		PRINT 과잉 노출
		LOCAL += 1
		IF LOCAL:1 ==1
			PRINT の
		ENDIF
	ENDIF
	IF EQUIP:601 == 1
		IF LOCAL ==1
			PRINT に
		ENDIF
		PRINT 大きなリボン
		LOCAL += 1
		IF LOCAL == LOCAL:1
			PRINT の付いた
		ENDIF
	ENDIF
	IF EQUIP:602 == 1
		IF LOCAL ==1
			PRINT と
		ELSEIF LOCAL ==2
			PRINT 、
		ENDIF
		PRINT きらめく宝石
		LOCAL += 1
		IF LOCAL == LOCAL:1
			PRINT を埋め込んだ
		ENDIF
	ENDIF
	IF EQUIP:603 == 1
		IF LOCAL ==1 || LOCAL==3
			PRINT と
		ELSEIF LOCAL ==2
			PRINT 、
		ENDIF
		PRINT 호사스러운 장식
		LOCAL += 1
		IF LOCAL == LOCAL:1 && (LOCAL ==2 || LOCAL==4)
			PRINT の付いた
		ELSE
			PRINT の
		ENDIF

	ENDIF
ENDIF
LOCAL = 0
SIF EQUIP:672 == 1
