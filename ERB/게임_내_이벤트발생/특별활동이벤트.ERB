				BASE:기력 = LIMIT(BASE:기력 - SYOUHI_KEIGEN(TARGET,LOCAL:130,기력), 0 , BASE:기력)
				LOCAL:121 = MAXBASE:체력 - BASE:체력
				LOCAL:131 = LOCAL:151 * LOCAL:121 / 1000 + RAND:10 + 200 + ABL:레벨 * 10
				MONEY += LOCAL:131
				;ヒーローショー失敗
			CASE 1
				LOCAL:120 = LOCAL:120 * (40 + RAND:7) / 100
				LOCAL:130 = LOCAL:130 * (50 + RAND:4) / 100
