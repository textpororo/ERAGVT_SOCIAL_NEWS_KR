;매료경험に対するFEATの効果（アイカツ）
@CALC_CHARM_FEAT_IDOL(対象, ARG)
#FUNCTION
#DIM 対象
SIF RAND:4 == 0
	RETURNF 0
LOCAL = 0
IF ARG > 1
	SIF TALENT:対象:평범
		LOCAL -= RAND:ARG + 1
	SIF TALENT:対象:남다른미모
		LOCAL += RAND:ARG + 1
ELSE
	SIF TALENT:対象:평범
		LOCAL -= 1
	SIF TALENT:対象:남다른미모
		LOCAL += 1
ENDIF
RETURNF LOCAL

;매료경험に対するFEATの効果（ヒーローショー・情報収集・自由行動）
@CALC_CHARM_FEAT_OTHER(対象)
#FUNCTION
#DIM 対象
LOCAL = 0
IF RAND:2 == 0 && EXP:対象:매료경험 >= 30 && EXP:対象:매료경험 != 99
	LOCAL += 1
	SIF TALENT:対象:평범 > 0 && RAND:4 == 0
		LOCAL -= 1
	SIF TALENT:対象:남다른미모 > 0 && RAND:4 == 0
		LOCAL += 1
ELSEIF RAND:3 == 0 && EXP:対象:매료경험 < 28
	LOCAL += 2
	SIF TALENT:평범 > 0 && RAND:4 != 0
		LOCAL -= 1
	SIF TALENT:対象:남다른미모 > 0 && RAND:4 != 0
		LOCAL += 1
ELSEIF EXP:対象:매료경험 < 29
	LOCAL += 1
	SIF TALENT:対象:평범 > 0 && RAND:4 == 0
		LOCAL -= 1
	SIF TALENT:対象:남다른미모 > 0 && RAND:4 != 0
		LOCAL += 1
ENDIF
;下積み（初回）がまだの時は매료경험が最高29になるように
IF EXP:対象:매료경험 < 30 && (EXP:対象:매료경험 + LOCAL >= 30)
	LOCAL = 29 - EXP:対象:매료경험
	;下積みの時は매료경험が最低30・最高99になるように
ELSEIF INRANGE(EXP:対象:매료경험, 30, 99)
	IF EXP:対象:매료경험 + LOCAL < 30
		LOCAL = 30 - EXP:対象:매료경험
	ELSEIF EXP:対象:매료경험 + LOCAL >= 100
		LOCAL = 99 - EXP:対象:매료경험
	ENDIF
	;デビュー済みの時は매료경험が最低100になるように
ELSEIF EXP:対象:매료경험 >= 100 && (EXP:対象:매료경험 + LOCAL < 100)
	LOCAL = 100 - EXP:対象:매료경험
ENDIF
RETURNF LOCAL

@IDOL_LV(対象)
#FUNCTION
#DIM 対象
;下積み（初回）
SIF EXP:対象:매료경험 < 30
	RETURNF 0
;下積み
SIF EXP:対象:매료경험 < 100
	RETURNF 1
;デビュー済み
RETURNF 2
