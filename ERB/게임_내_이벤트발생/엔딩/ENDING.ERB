	CALL ENDING_2
	$START_SCORE
	FLAG:999 = 0
	RESETBGCOLOR
	CALL SCORE
	FLAG:64 = RESULT
	PRINTL 클리어 데이터를 기록하시겠습니까？
	PRINTL [0] 네
	PRINTL [1] 아니오
	$INPUT_LOOP_2_0
	INPUT
	IF RESULT == 0
		SAVEGAME
		PRINTL 
	ELSEIF RESULT == 1
		PRINTL 
	ELSE
		GOTO INPUT_LOOP_2_0
	ENDIF
	$START_SUCCESSION
	;인계 개시 전에 시설 관련 자금을 환원
	;시설
	REPEAT FLAG:50
		SIF COUNT == 0
			CONTINUE
		MONEY += 5000 * COUNT + 5000
	REND
	REPEAT FLAG:51
		SIF COUNT == 0
			CONTINUE
		MONEY += 1000 * COUNT + 1000
	REND
	REPEAT FLAG:52
		MONEY += 10000 * COUNT + 10000
	REND
	SIF (FLAG:53 & 관엽식물)
		MONEY += 500
	SIF (FLAG:53 & 질좋은침대)
		MONEY += 12000
	SIF (FLAG:53 & 최고급침대)
		MONEY += 95000
	SIF (FLAG:53 & 안마의자)
		MONEY += 25800
	SIF (FLAG:53 & 목욕설비)
		MONEY += 50000
	SIF (FLAG:53 & 게임코너)
		MONEY += 100000
	SIF (FLAG:53 & 대욕탕)
		MONEY += 200000
	SIF (FLAG:53 & 미용온천)
		MONEY += 500000
	SIF (FLAG:53 & 뷰티살롱)
		MONEY += 1500000
	SIF (FLAG:53 & 마음이편안해지는풍경화)
		MONEY += 4500
	SIF (FLAG:53 & 세련된오브제)
		MONEY += 8500
	SIF (FLAG:53 & 호화로운분수)
