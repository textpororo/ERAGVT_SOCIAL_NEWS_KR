		CALL CLEARRANDCHOOSE
		FOR LOCAL, 0, 10
			CALL ADDRANDCHOOSE,(LOCAL + 1)
		NEXT
		FOR LOCAL, 0, MIN(FLAG:70 + FLAG:71 , 5)
			SIF CHOICECOUNT_F() == 0
				BREAK
			RESULT = RANDCHOOSE_F()
			IF RESULT == 10
				PRINTFORM 「%LOCALS%だ・・・%LOCALS%が来てくれた！！」
			ELSEIF RESULT == 9
				PRINTFORM 「そんなやつ、ぶっとばしちゃえ！」
			ELSEIF RESULT == 8
