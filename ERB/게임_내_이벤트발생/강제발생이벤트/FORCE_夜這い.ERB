	;보추を남자扱いしないように追記
	IF (ISPENIS()) && (ISGIRLY(LCOUNT))
		LOCAL:1 = ABL:C감각 + ABL:사정중독
		;연인関係なら選択確率上昇
		SIF LOVER_F(TARGET, LCOUNT) > 0 || LOVER_F(LCOUNT, TARGET) > 0
			LOCAL:1 *= 2
		;自分が보추なら選択確率低下
		IF TALENT:보추 > 0
			LOCAL:1 /= 3
			;보추で음핵、음고、음유を持っているならさらに選択確率低下
			SIF (TALENT:음핵 + TALENT:음고 + TALENT:음유) > 0
				LOCAL:1 /= 2
		ELSE
		ENDIF
		REPEAT LOCAL:1
			IF RAND:10 < (ABL:C감각 + ABL:사정중독)
				;性交を迫る
				CALL ADDRANDCHOOSE,16
			ELSE
				;手、口、胸による奉仕を迫る
				CALL ADDRANDCHOOSE,32
			ENDIF
		REND
	ENDIF
	;음핵を持っているかC감각Lv3以上の場合で남자でも후타나리でもない
	;보추を남자扱いしないように追記
	IF (TALENT:음핵 * 3 + ABL:C감각) >= 3 && TALENT:후타나리 < 1 && ISGIRLY()
		LOCAL:1 = ABL:C감각
		REPEAT LOCAL:1
			;Ｃ系夜這い
			CALL ADDRANDCHOOSE,1
		REND
	ENDIF
	;음호を持っているかV감각Lv3の場合で남자でない
	IF (TALENT:음호 * 3 + ABL:V감각) >= 3 && ISFEMALE()
		LOCAL:1 = ABL:V감각
		;相手がペニスを持っているなら選択確率上昇
		SIF TALENT:LCOUNT:후타나리 > 0 || ISMALE(LCOUNT)
			LOCAL:1 *= 2
		REPEAT LOCAL:1
			;Ｖ系夜這い
			CALL ADDRANDCHOOSE,2
		REND
	ENDIF
	;음고を持っているかA감각Lv3以上の場合で남자でない
	;보추を남자扱いしないように追記
	IF (TALENT:음고 * 3 + ABL:A감각) >= 3 && ISGIRLY()
		LOCAL:1 = ABL:A감각
		REPEAT LOCAL:1
			;Ａ系夜這い
			CALL ADDRANDCHOOSE,4
		REND
	ENDIF
	;음유を持っているかB감각Lv3以上の場合で남자でない
	;보추を남자扱いしないように追記
	IF (TALENT:음유 * 3 + ABL:B감각) >= 3 && ISGIRLY()
		LOCAL:1 = ABL:B감각
		REPEAT LOCAL:1
			;Ｂ系夜這い
			CALL ADDRANDCHOOSE,8
		REND
	ENDIF

	;候補がある場合
	IF ARG == 1
		LOCAL = 200
	ELSEIF ARG == 10
		LOCAL = 300
	ELSEIF ARG == 11
		LOCAL = 400
	ELSE
		LOCAL = 100
	ENDIF
	SIF CHOICECOUNT_F() > 0
		GFLAG:(LOCAL + LCOUNT) = RANDCHOOSE_F()
NEXT
