;---------------------------------------------------------------------
;特定の素質を持っていると仲間に夜這いを掛ける

;※このファイルのみ性別判定が古いまま(直接参照)です。
;　現在の変身状態と異なる性別も含めて網羅的に参照しており、分岐が複雑なため手を付けられていません。
;　きっとそのうち誰かがやる

@YOBAI
#DIM CCOUNT
;昼間は判定しない
SIF TIME == 0
	RETURN
;キャラが一人しかいない場合は判定しない
SIF CHARANUM_ACTIVE() < 2
	RETURN
;夜這い対象にできるキャラが居ない場合は判定しない
LOCAL = 0
FOR CCOUNT, 0, CHARANUM
	SIF CCOUNT == MASTER
		CONTINUE
	;パーティに参加していないキャラは除外
	SIF CFLAG:CCOUNT:999 == 0
		CONTINUE
	;ここまで抜けたら人数にカウント
	LOCAL ++
NEXT
SIF LOCAL == 0
	RETURN
;ここまで抜けてくれば判定を開始
VARSET LOCAL
SETCOLOR 105,105,105
SIF FLAG:999 == 1
	PRINTL ------ 夜這い判定 ------
RESETCOLOR
;ランダム抽選用に候補をクリア
CALL CLEARRANDCHOOSE
FOR CCOUNT, 1, CHARANUM
	SIF CFLAG:CCOUNT:999 == 0
		CONTINUE
	;能動的に動けない場合は除外
	SIF TALENT:CCOUNT:번식자루 > 0 || TALENT:CCOUNT:사지결손 > 0
		CONTINUE

	TARGET = CCOUNT
	LOCAL = 0

	;음핵、淫壺、음고、음유、후타나리、남자の場合に発生判定を行う
	IF (TALENT:음핵 * 3 + ABL:C감각 >= 3) || (TALENT:음호 * 3 + ABL:V감각 >= 3) || (TALENT:음고 * 3 + ABL:A감각 >= 3) || (TALENT:음유 * 3 + ABL:B감각 >= 3) || ((TALENT:후타나리 > 0 || TALENT:변신시후타나리 > 0 || ISMALE()) && ABL:사정중독 > 0)
		;生存していないキャラは判定しない
		SIF CFLAG:0 != 0
			CONTINUE
		;確率判定
		LOCAL:1 = (TALENT:음란 > 0) * 2 + (TALENT:음핵 * 3 + ABL:C감각 >= 3) + (TALENT:음호 * 3 + ABL:V감각 >= 3) + (TALENT:음고 * 3 + ABL:A감각 >= 3) + (TALENT:음유 * 3 + ABL:B감각 >= 3) + ((TALENT:후타나리 > 0 || TALENT:변신시후타나리 > 0  || ISMALE()) && ABL:사정중독 > 0)
		LOCAL:2 = MAX(ABL:욕망 + ABL:자위중독 + ABL:촉수중독 , 0)
		SIF LOCAL:1
			LOCAL = (LOCAL:1 * 2 + 22) * (125 + LOCAL:2 * 10)
		SIF TALENT:교제상대 >= 1 && TALENT:교제상대 < 5
			TIMES LOCAL, 0.5
		SIF TALENT:청순파
			TIMES LOCAL, 0.5
		SIF TALENT:인간불신 > 0
			TIMES LOCAL, 0.25
		IF FLAG:999 == 1
			SETCOLOR 105,105,105
			PRINTFORML 実行判定　%CALLNAME:TARGET%　確率{LOCAL}
			RESETCOLOR
		ENDIF
		SIF RAND:10000 < LOCAL
			CALL ADDRANDCHOOSE,TARGET
	ELSE
		CONTINUE
	ENDIF
NEXT
$REROLL
;候補がある場合
IF CHOICECOUNT_F()
	TARGET = RANDCHOOSE_F()
	CALL YOBAI_EVENT
	IF RESULT == - 999
		CALL CLEARSPECIFICCHOOSE(TARGET)
		SETCOLOR 105,105,105
		SIF FLAG:999 == 1
			PRINTFORML %CALLNAME:TARGET% = -999 / REROLL
		RESETCOLOR
		GOTO REROLL
	ENDIF
	;실적
	CALL GET_STATE_EXPUP,TARGET
	;모든 캐릭터의 変身を解除
	FOR LOCAL, 0, CHARANUM
		SIF LOCAL == MASTER
			CONTINUE
		SIF CFLAG:LOCAL:1 > 0 && CFLAG:LOCAL:0 == 0
			CALL TRANSFORM, 0, LOCAL
	NEXT
ELSE
	SETCOLOR 105,105,105
	SIF FLAG:999 == 1
		PRINTFORML 夜這い実行なし
	RESETCOLOR
ENDIF

;---------------------------------------------------------------------
;夜這いイベントの制御
@YOBAI_EVENT
#DIM LCOUNT
#DIM CCOUNT
#DIM SELECT,12
SIF CHARANUM_ACTIVE() == 0
	RETURN
;초기화
VARSET LOCAL
VARSET GFLAG
;現段階で対象者が居るかチェック
;双方が変身しなかった場合
SELECT = 0
;対象だけが変身した場合
SELECT:1 = 0
;実行者だけが変身した場合
SELECT:10 = 0
;双方が変身した場合
SELECT:11 = 0
FOR CCOUNT, 0, CHARANUM
	SIF CCOUNT == MASTER
		CONTINUE
	;実行者は除外
	SIF CCOUNT == TARGET
		CONTINUE
	;パーティに参加していないキャラは除外
	SIF CFLAG:CCOUNT:999 == 0
		CONTINUE

	;双方が変身しなかった場合
	;双方が남자でなければカウント
	;보추を남자扱いしないように追記
	SIF ISGIRLY(TARGET) || ISGIRLY(CCOUNT)
		SELECT:0 ++

	;相手だけが変身した場合
	;双方が남자でなければカウント
	;보추を남자扱いしないように追記
	SIF ISGIRLY(TARGET) || CHARATALENT_F(CCOUNT,1,"남자") == 0 || CHARATALENT_F(CCOUNT,1,"보추") > 0
		SELECT:1 ++

	;実行者だけが変身した場合
	;双方が남자でなければカウント
	;보추を남자扱いしないように追記
	SIF CHARATALENT_F(TARGET,1,"남자") == 0 || CHARATALENT_F(TARGET,1,"보추") > 0 || ISGIRLY(CCOUNT)
		SELECT:10 ++

	;双方が変身した場合
	;双方が남자でなければカウント
	;보추を남자扱いしないように追記
	SIF CHARATALENT_F(TARGET,1,"남자") == 0 || CHARATALENT_F(TARGET,1,"보추") > 0 || CHARATALENT_F(CCOUNT,1,"남자") == 0 || CHARATALENT_F(CCOUNT,1,"보추") > 0
		SELECT:11 ++

	;夜這い条件を満たしていない場合
	;보추を남자扱いしないように追記（후타나리と同じ判定）
	SETCOLOR 105,105,105
	IF (CHARATALENT_F(TARGET,0,"후타나리") > 0 || CHARATALENT_F(TARGET,0,"보추") > 0) && (TALENT:음핵 * 3 + ABL:C감각 < 3) && (TALENT:음호 * 3 + ABL:V감각 < 3) && (TALENT:음고 * 3 + ABL:A감각 < 3) && (TALENT:음유 * 3 + ABL:C감각 < 3) && ABL:C감각 + ABL:사정중독 == 0
		SELECT:0 = 0
		SELECT:1 = 0
	ELSEIF CHARATALENT_F(TARGET,0,"남자") == 0 && (TALENT:음핵 * 3 + ABL:C감각 < 3) && (TALENT:음호 * 3 + ABL:V감각 < 3) && (TALENT:음고 * 3 + ABL:A감각 < 3) && (TALENT:음유 * 3 + ABL:C감각 < 3)
		SELECT:0 = 0
		SELECT:1 = 0
	ELSEIF CHARATALENT_F(TARGET,0,"남자") > 0 && CHARATALENT_F(TARGET,0,"보추") == 0 && ABL:C감각 + ABL:사정중독 == 0
		SELECT:0 = 0
		SELECT:1 = 0
	ENDIF

	IF (CHARATALENT_F(TARGET,1,"후타나리") > 0 || CHARATALENT_F(TARGET,1,"보추") > 0) && (TALENT:음핵 * 3 + ABL:C감각 < 3) && (TALENT:음호 * 3 + ABL:V감각 < 3) && (TALENT:음고 * 3 + ABL:A감각 < 3) && (TALENT:음유 * 3 + ABL:C감각 < 3) && ABL:C감각 + ABL:사정중독 == 0
		SELECT:10 = 0
		SELECT:11 = 0
	ELSEIF CHARATALENT_F(TARGET,1,"남자") == 0 && (TALENT:음핵 * 3 + ABL:C감각 < 3) && (TALENT:음호 * 3 + ABL:V감각 < 3) && (TALENT:음고 * 3 + ABL:A감각 < 3) && (TALENT:음유 * 3 + ABL:C감각 < 3)
		SELECT:10 = 0
		SELECT:11 = 0
	ELSEIF CHARATALENT_F(TARGET,1,"남자") > 0 && CHARATALENT_F(TARGET,1,"보추") == 0 && ABL:C감각 + ABL:사정중독 == 0
		SELECT:10 = 0
		SELECT:11 = 0
	ENDIF

	IF CHARATALENT_F(TARGET,0,"남자") == 0 && CHARATALENT_F(TARGET,1,"남자") > 0 && TALENT:임신 > 0
		SELECT:10 = 0
		SELECT:11 = 0
	ENDIF

	IF CHARATALENT_F(CCOUNT,0,"남자") == 0 && CHARATALENT_F(CCOUNT,1,"남자") > 0 && TALENT:CCOUNT:임신 > 0
		SELECT:1 = 0
		SELECT:11 = 0
	ENDIF

	;여체수용なし
	;보추を남자扱いしないように追記
	IF CHARATALENT_F(TARGET,0,"남자") > 0 && CHARATALENT_F(TARGET,0,"보추") == 0 && TALENT:변신시TS > 0 && TALENT:여체수용 == 0
		SELECT:10 = 0
		SELECT:11 = 0
	ENDIF

	;性別嗜好による選別
	;보추を남자扱いしないように追記（迷ったが、夜這いをかけるほどムラムラしてるなら女扱いされるだろうと判断）
	IF ISMANLY(CCOUNT) && TALENT:남성기피 > 0 && TALENT:양도 == 0 && RAND:4 == 0
		SELECT:0 = 0
		SELECT:1 = 0
		SELECT:10 = 0
		SELECT:11 = 0
		SIF FLAG:999 == 1
			PRINTL 남성기피
	ENDIF
	IF ISFEMALE(CCOUNT) && TALENT:여성기피 > 0 && TALENT:양도 == 0 && RAND:4 == 0
		SELECT:0 = 0
		SELECT:1 = 0
		SELECT:10 = 0
		SELECT:11 = 0
		SIF FLAG:999 == 1
			PRINTL 여성기피
	ENDIF
	RESETCOLOR
NEXT
;debug
SETCOLOR 105,105,105
SIF FLAG:999 == 1
	PRINTFORML 夜這いプレイ内容抽選 %CALLNAME:TARGET%　{SELECT} / {SELECT:1} / {SELECT:10} / {SELECT:11}
RESETCOLOR
;対象人数が0人の場合、リロール
SIF SELECT + SELECT:1 + SELECT:10 + SELECT:11 == 0
	RETURN -999

;プレイ内容の抽選
;双方が変身しない場合
SIF SELECT > 0
	CALL YOBAI_SELECT_PLAY, 0
;対象のみが変身した場合
SIF SELECT:1 > 0
	CALL YOBAI_SELECT_PLAY, 1
;実行者のみが変身した場合
SIF SELECT:10 > 0
	CALL YOBAI_SELECT_PLAY, 10
;双方が変身した場合
SIF SELECT:11 > 0
	CALL YOBAI_SELECT_PLAY, 11

;キャラリスト印刷開始
DRAWLINE
FONTBOLD
PRINTL （夜這い対象選択）
FONTREGULAR
PRINTL 
PRINTFORML %PRINT_CALLNAME(TARGET)%は眠れぬ夜を過ごしている・・・
;実行者がＴＳしなければ対象が居ない場合
;実行者が보추変身しなければ対象が居ない場合も追記
IF SELECT:0 + SELECT:1 == 0
	IF TALENT:여체수용 > 0 || TALENT:변신시보추 > 0
		IF TALENT:변신시보추 > 0
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%変身で보추化できます。変身した状態で夜這いを行いますか？
		ELSE
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%変身で女体化できます。変身した状態で夜這いを行いますか？
		ENDIF
		PRINTFORML  [0] 네　[1]夜這いを行わずに終了する
		$INPUT_LOOP_0_0
		INPUT
		IF RESULT == 0
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%変身した！
			PRINTW 
			CALL TRANSFORM, 1
		ELSEIF RESULT == 1
			PRINTFORML 夜這いを終了します。
			PRINTW 
			;残りの処理を行わずに終了
			RETURN -1
		ELSE
			GOTO INPUT_LOOP_0_0
		ENDIF
	ELSE
		;候補が居ないのでリロール
		RETURN -999
	ENDIF
	;ＴＳした際に対象が居る場合
ELSEIF SELECT:10 + SELECT:11 > 0
	IF ((TALENT:변신시TS > 0 || TALENT:변신시후타나리 > 0) && (ISFEMALE() || TALENT:여체수용 > 0)) || TALENT:변신시보추 > 0
		IF TALENT:변신시보추 > 0
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%変身で보추化できます。変身した状態で夜這いを行いますか？
		ELSEIF ISMALE()
			PRINTFORML %조사처리(PRINT_TRANSCALLNAME(TARGET),"는")%変身で女体化できます。変身した状態で夜這いを行いますか？
