;-------------------------------
PRINTW 
PRINTL 
PRINTFORML 電車を待つ%PRINT_TRANSCALLNAME(TARGET)%의 行き先は・・・
PRINTL 
PRINT [0]動物園　　　　　　　　
PRINT [1]水族館　　　　　　　　
PRINTL 
PRINT [2]遊園地　　　　　　　　
PRINT [3]植物園　　　　　　　　
PRINTL 
PRINT [4]ライブ　　　　　　　　
PRINT [5]ウォーターパーク　　　
PRINTL 
PRINT [6]海辺　　　　　　　　　
PRINT [7]温泉　　　　　　　　　
PRINTL 
;PRINT [8]何か思いついた時用　　
;PRINT [9]何か思いついた時用　　
PRINTL 
DRAWLINE
$INPUT_LOOP
INPUT
PRINTL 
DRAWLINE
IF RESULT == 0
	PRINTL 動物園
ELSEIF RESULT == 1
	PRINTL 水族館
ELSEIF RESULT == 2
	PRINTL 遊園地
ELSEIF RESULT == 3
	PRINTL 植物園
ELSEIF RESULT == 4
	PRINTL ライブ
ELSEIF RESULT == 5
	PRINTL ウォーターパーク
ELSEIF RESULT == 6
	PRINTL 海辺
ELSEIF RESULT == 7
	PRINTL 温泉
ELSE
	PRINTL 올바른 값을 입력해 주십시오.
	GOTO INPUT_LOOP
ENDIF
LOCAL = RESULT
PRINTL 

PRINTL 
CALL DOT_AFTER, 1
PRINTL 
CALL PASTIME_CHIKAN, ARG	
IF RESULT == 0
	IF LOCAL == 0
		目的地 = 動物園
		CALL MESSAGE_PASTIME_Zoo
	ELSEIF LOCAL == 1
		目的地 = 水族館
		CALL MESSAGE_PASTIME_Aquarium
	ELSEIF LOCAL == 2
		目的地 = 遊園地
		CALL MESSAGE_PASTIME_AmusementPark
	ELSEIF LOCAL == 3
		目的地 = 植物園
		CALL MESSAGE_PASTIME_BotanicalGarden
	ELSEIF LOCAL == 4
		目的地 = ライブ
		CALL MESSAGE_PASTIME_LiveShow
	ELSEIF LOCAL == 5
		目的地 = ウォーターパーク
		CALL MESSAGE_PASTIME_WaterPark
	ELSEIF LOCAL == 6
		目的地 = 海辺
		CALL MESSAGE_PASTIME_BeachSide
	ELSEIF LOCAL == 7
		目的地 = 温泉
		CALL MESSAGE_PASTIME_Spa
	ENDIF

	;ナンパ判定（남자非対応）
	;보추対応済み
	CALL PASTIME_NANPA
	IF RESULT > 0
		;繁華街と同じナンパ
		CALL MESSAGE_PASTIME_NANPA,ARG
	;防衛度依存・淫気（男女）
	ELSEIF RAND:100 > FLAG:852 / 100	
		CALL PASTIME_INKIOKYU
	ELSE
		PRINTFORML %PRINT_TRANSCALLNAME(TARGET)%은(는)、%目的地%を楽しんできたようだ。
		PRINTW 
	ENDIF
ENDIF

